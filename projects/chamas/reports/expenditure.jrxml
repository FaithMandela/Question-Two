	<queryString>
		<![CDATA[SELECT vw_expenditure.expenditure_id,
 vw_expenditure.pc_item_name,
 vw_expenditure.default_units,
 vw_expenditure.units,
 vw_expenditure.default_price,
 vw_expenditure.unit_price,
 (vw_expenditure.default_units * vw_expenditure.default_price) AS estimated_cost,
 (vw_expenditure.units * vw_expenditure.unit_price) AS actual_cost,
  vw_expenditure.exp_date
FROM vw_expenditure
 WHERE vw_expenditure.exp_date >= ('$P!{start_date}' :: date)
 AND vw_expenditure.exp_date<= ('$P!{end_date}' :: date)
 ORDER BY vw_expenditure.expenditure_id;]]>
