		<defaultValueExpression><![CDATA["/opt/apache-tomcat-7.0.30/webapps/cases/reports/"]]></defaultValueExpression>
	</parameter>
	<parameter name="filterid" class="java.lang.String">
		<defaultValueExpression><![CDATA["0"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT sys_countrys.sys_country_name,
	clients.client_id, clients.client_name,
	clients.address, clients.zipcode,
	clients.premises, clients.street,
	clients.division, clients.town,
	clients.pcc, clients.iatano,
	clients.telno, clients.email, clients.website,
	clients.travel_manager, clients.travel_manager_email,
	clients.technical_contact, clients.technical_contact_email,

	pm_schedule.pm_schedule_id, pm_schedule.pm_quarter_id,
	pm_schedule.start_date, pm_schedule.end_date,
	pm_schedule.pm_group, pm_schedule.date_done,
	pm_schedule.completed, pm_schedule.approved,
	pm_schedule.contact_person

FROM clients INNER JOIN sys_countrys ON clients.country_id = sys_countrys.sys_country_id
	INNER JOIN pm_schedule ON clients.client_id = pm_schedule.client_id

WHERE (pm_schedule.pm_schedule_id  = '$P!{filterid}')]]>
	</queryString>
