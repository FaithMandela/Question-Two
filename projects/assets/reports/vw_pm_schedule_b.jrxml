	<subDataset name="QLinks">
		<parameter name="client_id" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT entity_name, ip_allocation, connection_type, link_capacity, link_number
FROM vw_client_links
WHERE (client_id = $P{client_id})]]>
		</queryString>
		<field name="entity_name" class="java.lang.String"/>
		<field name="ip_allocation" class="java.lang.String"/>
		<field name="connection_type" class="java.lang.String"/>
		<field name="link_capacity" class="java.lang.Integer"/>
		<field name="link_number" class="java.lang.String"/>
	</subDataset>
	<subDataset name="QAssets">
		<parameter name="clientID" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[1]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT asset_type_name, manufacturer_name, model_name, asset_serial, date_issued
FROM vw_client_assets

WHERE (is_issued = true) AND (is_retrived = false)
AND (client_id = $P{clientID})

ORDER BY asset_type_name, asset_serial]]>
		</queryString>
		<field name="asset_type_name" class="java.lang.String"/>
		<field name="manufacturer_name" class="java.lang.String"/>
		<field name="model_name" class="java.lang.String"/>
		<field name="asset_serial" class="java.lang.String"/>
		<field name="date_issued" class="java.sql.Date"/>
	</subDataset>
