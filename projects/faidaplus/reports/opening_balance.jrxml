	<queryString>
		<![CDATA[SELECT sum(a.dr)as points, a.order_date::date AS order_date, sum(a.segments)as segments,
a.org_name, a.entity_id, a.entity_name, sum(a.dr - a.cr) AS balance, sum(a.amount)as amount FROM ((SELECT COALESCE(vw_son_points.points, 0::real) + COALESCE(vw_son_points.bonus, 0::real) AS dr,
0::real AS cr, vw_son_points.period AS order_date, vw_son_points.son,
vw_son_points.pcc, vw_son_points.org_name, vw_son_points.entity_id,vw_son_points.entity_name,
	COALESCE(vw_son_points.bonus, 0::real) AS bonus, COALESCE(vw_son_points.segments, 0::real) AS segments, COALESCE(vw_son_points.amount, 0::real) AS amount
	FROM vw_son_points)
	UNION
	(SELECT 0::real AS float4, vw_orders.order_total_amount::real AS order_total_amount,
		vw_orders.order_date, vw_orders.son, vw_orders.pcc, vw_orders.org_name,
		vw_orders.entity_id,vw_orders.entity_name, 0::real AS bonus, 0::real AS segments,
0::real AS amount
	FROM vw_orders)) a
GROUP BY  a.order_date::date,a.org_name, a.entity_id, a.entity_name;]]>
	</queryString>
