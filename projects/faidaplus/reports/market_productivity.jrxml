	<queryString>
		<![CDATA[SELECT  period_id,to_char(period,'Mon yyyy')as period,
	ticket_period,
	orgs.pcc, segments,
	points, bonus, orgs.org_name
	FROM vw_org_points
	JOIN orgs ON vw_org_points.org_id = orgs.org_id
	WHERE period >= '$P!{minDate}'
	 AND period <= '$P!{maxDate}'
ORDER BY period_id DESC;]]>
	</queryString>
