	</parameter>
	<queryString>
		<![CDATA[SELECT vw_son_points.user_name,
     sum(vw_son_points.segments) as segments, sum(vw_son_points.points+vw_son_points.bonus) as totalkes,
    sum(vw_son_points.bonus) as total_bonus, sum(vw_orders.grand_total) as order_total_amount,
    sum(vw_son_points.points+vw_son_points.bonus - vw_orders.grand_total ) as balance
   FROM vw_son_points
     JOIN vw_orders ON vw_son_points.entity_id = vw_orders.entity_id
    WHERE to_char(vw_orders.order_date, 'mmyyyy'::text) >= to_char('$P!{minDate}'::date, 'mmyyyy'::text)
    AND to_char(vw_orders.order_date, 'mmyyyy'::text) <= to_char('$P!{maxDate}'::date, 'mmyyyy'::text)
     GROUP BY vw_son_points.user_name;]]>
	</queryString>
