	<queryString>
		<![CDATA[SELECT org_id , entity_id, entity_name, date, contribution, loans, repayments, investments FROM

((select org_id , entity_id, entity_name, entry_date AS date, contribution_amount AS contribution, 0::real AS loans, 0::real AS repayments, 0::real AS investments from vw_contributions)
UNION
(select org_id,  entity_id, entity_name, loan_date, 0::real, principle, 0::real, 0::real from vw_loans where vw_loans.approve_status = 'Approved' )
UNION
(select org_id , entity_id, entity_name, end_date, 0::real, 0::real, total_repayment, 0::real from vw_loan_monthly where vw_loan_monthly.approve_status = 'Approved')
UNION
(select org_id, entity_id, entity_name, maturity_date, 0::real, 0::real, 0::real, invest_amount from vw_investments)) AS a
$P!{orgwhere}
ORDER BY date DESC

]]>
	</queryString>
