	<queryString>
		<![CDATA[SELECT
    periods.start_date AS period,
    vw_entitys.client_code,  sum(vw_loyalty_points.segments)as segments,sum(vw_loyalty_points.points)as points,
    sum(vw_loyalty_points.bonus)as bonus, vw_entitys.org_name, vw_entitys.entity_name,vw_entitys.user_name,
    vw_entitys.is_active,  vw_entitys.entity_id
   FROM vw_loyalty_points
     JOIN vw_entitys ON vw_loyalty_points.entity_id = vw_entitys.entity_id
     JOIN periods ON vw_loyalty_points.period_id = periods.period_id
  WHERE   vw_entitys.is_active = true
  GROUP BY periods.start_date,vw_entitys.client_code, vw_entitys.org_name,vw_entitys.entity_name,
    vw_entitys.user_name,  vw_entitys.is_active,  vw_entitys.entity_id ;]]>
	</queryString>
