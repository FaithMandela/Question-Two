	<REPORT name="Application Confirmation" keyfield="applicantid" action="ApplicantEmailed"
			actioncount="1" table="applicants, webshow"
			where="(applicants.emailed[1] = false) AND (webshow.webshowid = 1)">
		<SUBJECT title="subject" basetable="webshow">narrative</SUBJECT>
		<ADDRESS title="address" basetable="applicants">Email</ADDRESS>
		<BODYFIELD reference="{{applicantname}}" basetable="applicants">companyname</BODYFIELD>
		<FIELD title="Notification" basetable="webshow">details</FIELD>
