	<queryString><![CDATA[SELECT vwledger.transactionid, vwledger.transactiondate, vwledger.clientid, vwledger.clientname, 
	vwledger.tradername, vwledger.transactionsource, vwledger.sourceid, vwledger.narrative, 
	vwledger.cramount, vwledger.dramount, vwledger.balance, 
	vwperoids.yearid, vwperoids.monthname,
	getbalance(vwledger.clientid, vwperoids.startdate) as openingbalance
FROM vwledger INNER JOIN vwperoids ON to_char(vwledger.transactiondate, 'YYYYMM') = vwperoids.monthid
WHERE (vwperoids.monthid = '$P!{filterid}') AND (clientid = '$P!{clientid}')
ORDER BY vwledger.transactionid;]]></queryString>

