<APP name="Fidelity" password="change_password">
	<MENU name="Fidelity">
		<MENU name="Account Opening">1</MENU>
	</MENU>
	<DESK w="700" name="Account Opening" key="1" h="550">
		<FORM new="true" edit="false" delete="false" audit="false" fornew="true" name="Account Openings" keyfield="account_opening_id" table="account_openings" noorg="true">
			<COMBOBOX w="430" title="Account Type" lptable="account_types" noorg="true" y="10" h="20" x="10" lpfield="account_type_name">account_type_id</COMBOBOX>
			<COMBOBOX w="430" title="Corporate Type" lptable="corporate_types" noorg="true" y="30" h="20" x="10" lpfield="corporate_type_name">corporate_type_id</COMBOBOX>
			<COMBOBOX w="430" title="Currency" lptable="currency" noorg="true" y="30" h="20" x="10" lpfield="currency_name">currency_id</COMBOBOX>
			<TEXTFIELD w="150" title="Account Name" y="70" h="20" x="10" required="true">account_name</TEXTFIELD>
			<TEXTFIELD w="150" title="Business Address" y="70" h="20" x="290" required="true">business_address</TEXTFIELD>
			<TEXTFIELD w="150" title="City" y="90" h="20" x="10" required="true">city</TEXTFIELD>
			<TEXTFIELD w="150" title="State" y="90" h="20" x="290">state</TEXTFIELD>
			<TEXTFIELD w="150" title="Phone" y="110" h="20" x="10" required="true">phone</TEXTFIELD>
			<TEXTFIELD w="430" title="Email" y="130" h="20" x="10" type="email">email</TEXTFIELD>
			<TEXTFIELD w="430" title="Website" y="150" h="20" x="10">website</TEXTFIELD>
			<TEXTFIELD w="430" title="Primary Contact" y="170" h="20" x="10" required="true">primary_contact </TEXTFIELD>
			<TEXTFIELD w="430" title="Primary Email" y="190" h="20" x="10" required="true" type="email">primary_email</TEXTFIELD>
			<GRID name="Application" keyfield="account_opening_id" linkfield="account_opening_id" display="form" table="account_openings">
				<TEXTFIELD w="430" fnct="CAST('Data submited check your email for details' as varchar)" title="Submit" y="110" h="20" x="10">disp</TEXTFIELD>
			</GRID>
		</FORM>
	</DESK>

</APP>
