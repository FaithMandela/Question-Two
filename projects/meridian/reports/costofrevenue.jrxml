<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="costofrevenue" language="groovy" pageWidth="595" pageHeight="842" columnWidth="575" leftMargin="0" rightMargin="20" topMargin="0" bottomMargin="0">
	<queryString>
		<![CDATA[select gls.account_id as accountid, accounts.account_name as accountname,
sum(gls.debit-gls.credit) as amount
from gls
inner join journals on gls.journal_id = journals.journal_id
inner join periods on journals.period_id = periods.period_id
inner join accounts on gls.account_id=accounts.account_id
inner join account_types on accounts.account_type_id=account_types.account_type_id
inner join accounts_class on account_types.accounts_class_id=accounts_class.accounts_class_id
where accounts_class.chat_type_id=5
group by accountid,accountname
order by accountid]]>
	</queryString>
	<field name="accountid" class="java.lang.Integer">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="accountname" class="java.lang.String">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<field name="amount" class="java.lang.Float">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<variable name="totalcostofrevenue" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{amount}]]></variableExpression>
	</variable>
