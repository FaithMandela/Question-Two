<APP name="Mobile Health" org="org_id" password="change_password">
	<MENU name="Mobile Health">
		<MENU name="Entitys">1</MENU>
		<MENU name="Orgs">5</MENU>
	</MENU>
	<DESK w="700" name="Entitys" key="1" h="550">
		<GRID name="Entitys" keyfield="entity_id" table="vw_entitys">ESTAMP>
			<TEXTFIELD w="75" title="Org Name">org_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Entity ID">entity_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Entity Name">entity_name</TEXTFIELD>
			<TEXTFIELD w="75" title="User Name">user_name</TEXTFIELD>
			<CHECKBOX w="75" title="Active">is_active</CHECKBOX>
			<TEXTFIELD w="75" title="Primary Email">primary_email</TEXTFIELD>
			<TEXTFIELD w="75" title="Entity Type Name">entity_type_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Entity Role">entity_role</TEXTFIELD>
			<TEXTFIELD w="75" title="Use Key">use_key</TEXTFIELD>
			<FORM name="Entitys" keyfield="entity_id" table="entitys">
				<COMBOBOX w="430" title="Entity Type ID" lptable="entity_types" y="10" h="20" x="10" lpfield="entity_type_name">entity_type_id</COMBOBOX>
				<TEXTFIELD w="430" title="Entity Name" y="30" h="20" x="10">entity_name</TEXTFIELD>
				<TEXTFIELD w="430" title="User Name" y="50" h="20" x="10">user_name</TEXTFIELD>
				<TEXTFIELD w="430" title="Primary Email" y="70" h="20" x="10">primary_email</TEXTFIELD>
				<TEXTFIELD w="150" title="Primary Telephone" y="90" h="20" x="10">primary_telephone</TEXTFIELD>
				<CHECKBOX w="150" title="Super User" y="90" h="20" x="290">super_user</CHECKBOX>
				<CHECKBOX w="150" title="Entity Leader" y="110" h="20" x="10">entity_leader</CHECKBOX>
				<CHECKBOX w="150" title="No Org" y="110" h="20" x="290">no_org</CHECKBOX>
				<TEXTFIELD w="430" title="Function Role" y="130" h="20" x="10">function_role</TEXTFIELD>
				<TEXTTIMESTAMP w="150" title="Date Enroled" y="150" h="20" x="10">date_enroled</TEXTTIMESTAMP>
				<CHECKBOX w="150" title="Active" y="150" h="20" x="290">is_active</CHECKBOX>
				<TEXTFIELD w="150" title="Entity Password" y="170" h="20" x="10">entity_password</TEXTFIELD>
				<TEXTFIELD w="150" title="First Password" y="170" h="20" x="290">first_password</TEXTFIELD>
				<TEXTFIELD w="150" title="New Password" y="190" h="20" x="10">new_password</TEXTFIELD>
				<TEXTFIELD w="150" title="Start Url" y="190" h="20" x="290">start_url</TEXTFIELD>
				<CHECKBOX w="150" title="Picked" y="210" h="20" x="10">is_picked</CHECKBOX>
				<TEXTAREA w="430" title="Details" y="230" h="70" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
	</DESK>
	<DESK w="700" name="Orgs" key="5" h="550">
		<GRID name="Orgs" keyfield="org_id" table="vw_orgs">
			<TEXTFIELD w="75" title="Org ID">org_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Org Name">org_name</TEXTFIELD>
			<CHECKBOX w="75" title="Default">is_default</CHECKBOX>
			<CHECKBOX w="75" title="Active">is_active</CHECKBOX>
			<GRID name="Health Workers" keyfield="health_worker_id" linkfield="org_id" table="health_workers">
				<TEXTFIELD w="75" title="Health Worker ID">health_worker_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Org ID">org_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Worker Name">worker_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Worker National ID">worker_national_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Worker Mobile Num">worker_mobile_num</TEXTFIELD>
				<TEXTFIELD w="75" title="Worker Pass">worker_pass</TEXTFIELD>
				<CHECKBOX w="75" title="First Login">is_first_login</CHECKBOX>
				<CHECKBOX w="75" title="Active">is_active</CHECKBOX>
				<TEXTFIELD w="75" title="Device ID">device_id</TEXTFIELD>
				<TEXTTIMESTAMP w="75" title="Date Enrolled">date_enrolled</TEXTTIMESTAMP>
				<FORM name="Health Worker" keyfield="health_worker_id" linkfield="org_id" table="health_workers">
					<TEXTFIELD w="150" title="Worker Name" y="10" h="20" x="10">worker_name</TEXTFIELD>
					<TEXTFIELD w="150" title="Worker National ID" y="10" h="20" x="290">worker_national_id</TEXTFIELD>
					<TEXTFIELD w="150" title="Worker Mobile Num" y="30" h="20" x="10">worker_mobile_num</TEXTFIELD>
					<TEXTFIELD w="150" title="Worker Pass" y="30" h="20" x="290">worker_pass</TEXTFIELD>
					<CHECKBOX w="150" title="First Login" y="50" h="20" x="10">is_first_login</CHECKBOX>
					<CHECKBOX w="150" title="Active" y="50" h="20" x="290">is_active</CHECKBOX>
					<COMBOBOX w="430" title="Device Name" lptable="devices" y="70" h="20" x="10" lpfield="device_name">device_id</COMBOBOX>
					<TEXTTIMESTAMP w="150" title="Date Enrolled" y="90" h="20" x="10">date_enrolled</TEXTTIMESTAMP>
				</FORM>
			</GRID>
			<GRID name="Devices" keyfield="device_id" linkfield="org_id" table="devices">
				<TEXTFIELD w="75" title="Device ID">device_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Org ID">org_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Device Name">device_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Device Model">device_model</TEXTFIELD>
				<TEXTFIELD w="75" title="Device Imei 1">device_imei_1</TEXTFIELD>
				<TEXTFIELD w="75" title="Device Imei 2">device_imei_2</TEXTFIELD>
				<CHECKBOX w="75" title="Active">is_active</CHECKBOX>
				<FORM name="Device" keyfield="device_id" linkfield="org_id" table="devices">
					<TEXTFIELD w="150" title="Device Name" y="50" h="20" x="10">device_name</TEXTFIELD>
					<TEXTFIELD w="150" title="Device Model" y="10" h="20" x="10">device_model</TEXTFIELD>
					<TEXTFIELD w="150" title="Device Imei 1" y="10" h="20" x="290">device_imei_1</TEXTFIELD>
					<TEXTFIELD w="150" title="Device Imei 2" y="30" h="20" x="10">device_imei_2</TEXTFIELD>
					<CHECKBOX w="150" title="Active" y="30" h="20" x="290">is_active</CHECKBOX>
				</FORM>
			</GRID>
			<FORM name="Org" keyfield="org_id" table="orgs">
				<COMBOBOX w="430" title="Currency ID" lptable="currency" y="10" h="20" x="10" lpfield="currency_name">currency_id</COMBOBOX>
				<COMBOBOX w="430" title="Parent Org ID" lptable="orgs" lpkey="org_id" y="30" h="20" x="10" lpfield="org_name">parent_org_id</COMBOBOX>
				<TEXTFIELD w="150" title="Org Name" y="50" h="20" x="10">org_name</TEXTFIELD>
				<TEXTFIELD w="150" title="Org Sufix" y="50" h="20" x="290">org_sufix</TEXTFIELD>
				<CHECKBOX w="150" title="Default" y="70" h="20" x="10">is_default</CHECKBOX>
				<CHECKBOX w="150" title="Active" y="70" h="20" x="290">is_active</CHECKBOX>
				<TEXTFIELD w="150" title="Logo" y="90" h="20" x="10">logo</TEXTFIELD>
				<TEXTFIELD w="150" title="Pin" y="90" h="20" x="290">pin</TEXTFIELD>
				<TEXTAREA w="430" title="Details" y="110" h="70" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
	</DESK>
</APP>
