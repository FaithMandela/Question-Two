<APP name="Mobile Health" org="org_id" password="change_password">
	<MENU name="Mobile Health">
		<MENU name="Entitys">1</MENU>
		<MENU name="Orgs">5</MENU>
		<MENU name="Countys">10</MENU>
		<MENU name="Surveys">15</MENU>
	</MENU>
	<DESK w="700" name="Entitys" key="1" h="550">
		<GRID name="Entitys" keyfield="entity_id" table="vw_entitys">ESTAMP>
			<TEXTFIELD w="75" title="Org Name">org_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Entity ID">entity_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Entity Name">entity_name</TEXTFIELD>
			<TEXTFIELD w="75" title="User Name">user_name</TEXTFIELD>
			<CHECKBOX w="75" title="Active">is_active</CHECKBOX>
			<TEXTFIELD w="75" title="Primary Email">primary_email</TEXTFIELD>
			<TEXTFIELD w="75" title="Entity Type Name">entity_type_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Entity Role">entity_role</TEXTFIELD>
			<TEXTFIELD w="75" title="Use Key">use_key</TEXTFIELD>
			<FORM name="Entitys" keyfield="entity_id" table="entitys">
				<COMBOBOX w="430" title="Entity Type ID" lptable="entity_types" y="10" h="20" x="10" lpfield="entity_type_name">entity_type_id</COMBOBOX>
				<TEXTFIELD w="430" title="Entity Name" y="30" h="20" x="10">entity_name</TEXTFIELD>
				<TEXTFIELD w="430" title="User Name" y="50" h="20" x="10">user_name</TEXTFIELD>
				<TEXTFIELD w="430" title="Primary Email" y="70" h="20" x="10">primary_email</TEXTFIELD>
				<TEXTFIELD w="150" title="Primary Telephone" y="90" h="20" x="10">primary_telephone</TEXTFIELD>
				<CHECKBOX w="150" title="Super User" y="90" h="20" x="290">super_user</CHECKBOX>
				<CHECKBOX w="150" title="Entity Leader" y="110" h="20" x="10">entity_leader</CHECKBOX>
				<CHECKBOX w="150" title="No Org" y="110" h="20" x="290">no_org</CHECKBOX>
				<TEXTFIELD w="430" title="Function Role" y="130" h="20" x="10">function_role</TEXTFIELD>
				<TEXTTIMESTAMP w="150" title="Date Enroled" y="150" h="20" x="10">date_enroled</TEXTTIMESTAMP>
				<CHECKBOX w="150" title="Active" y="150" h="20" x="290">is_active</CHECKBOX>
				<TEXTFIELD w="150" title="Entity Password" y="170" h="20" x="10">entity_password</TEXTFIELD>
				<TEXTFIELD w="150" title="First Password" y="170" h="20" x="290">first_password</TEXTFIELD>
				<TEXTFIELD w="150" title="New Password" y="190" h="20" x="10">new_password</TEXTFIELD>
				<TEXTFIELD w="150" title="Start Url" y="190" h="20" x="290">start_url</TEXTFIELD>
				<CHECKBOX w="150" title="Picked" y="210" h="20" x="10">is_picked</CHECKBOX>
				<TEXTAREA w="430" title="Details" y="230" h="70" x="10">details</TEXTAREA>
			</FORM>
			<ACTIONS>
				<ACTION fnct="change_password">Reset Password</ACTION>
			</ACTIONS>
		</GRID>
	</DESK>
	<DESK w="700" name="Orgs" key="5" h="550">
		<GRID name="Orgs" keyfield="org_id" table="vw_orgs" noorg="true">
			<TEXTFIELD w="75" title="Org ID">org_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Org Name">org_name</TEXTFIELD>
			<CHECKBOX w="75" title="Default">is_default</CHECKBOX>
			<CHECKBOX w="75" title="Active">is_active</CHECKBOX>
			<GRID name="Health Workers" keyfield="health_worker_id" linkfield="org_id" table="health_workers">
				<TEXTFIELD w="75" title="Health Worker ID">health_worker_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Org ID">org_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Worker Name">worker_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Worker National ID">worker_national_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Worker Mobile Num">worker_mobile_num</TEXTFIELD>
				<TEXTFIELD w="75" title="Worker Pass">worker_pass</TEXTFIELD>
				<CHECKBOX w="75" title="First Login">is_first_login</CHECKBOX>
				<CHECKBOX w="75" title="Active">is_active</CHECKBOX>
				<TEXTFIELD w="75" title="Device ID">device_id</TEXTFIELD>
				<TEXTTIMESTAMP w="75" title="Date Enrolled">date_enrolled</TEXTTIMESTAMP>
				<FORM name="Health Worker" keyfield="health_worker_id" linkfield="org_id" table="health_workers">
					<TEXTFIELD w="150" title="Worker Name" y="10" h="20" x="10">worker_name</TEXTFIELD>
					<TEXTFIELD w="150" title="Worker National ID" y="10" h="20" x="290">worker_national_id</TEXTFIELD>
					<TEXTFIELD w="150" title="Worker Mobile Num" y="30" h="20" x="10">worker_mobile_num</TEXTFIELD>
					<TEXTFIELD w="150" title="Worker Pass" y="30" h="20" x="290">worker_pass</TEXTFIELD>
					<CHECKBOX w="150" title="First Login" y="50" h="20" x="10">is_first_login</CHECKBOX>
					<CHECKBOX w="150" title="Active" y="50" h="20" x="290">is_active</CHECKBOX>
					<COMBOBOX w="430" title="Device Name" lptable="devices" y="70" h="20" x="10" lpfield="device_name">device_id</COMBOBOX>
					<TEXTTIMESTAMP w="150" title="Date Enrolled" y="90" h="20" x="10">date_enrolled</TEXTTIMESTAMP>
				</FORM>
			</GRID>
			<GRID name="Devices" keyfield="device_id" linkfield="org_id" table="devices">
				<TEXTFIELD w="75" title="Device ID">device_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Org ID">org_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Device Name">device_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Device Model">device_model</TEXTFIELD>
				<TEXTFIELD w="75" title="Device Imei 1">device_imei_1</TEXTFIELD>
				<TEXTFIELD w="75" title="Device Imei 2">device_imei_2</TEXTFIELD>
				<CHECKBOX w="75" title="Active">is_active</CHECKBOX>
				<FORM name="Device" keyfield="device_id" linkfield="org_id" table="devices">
					<TEXTFIELD w="150" title="Device Name" y="50" h="20" x="10">device_name</TEXTFIELD>
					<TEXTFIELD w="150" title="Device Model" y="10" h="20" x="10">device_model</TEXTFIELD>
					<TEXTFIELD w="150" title="Device Imei 1" y="10" h="20" x="290">device_imei_1</TEXTFIELD>
					<TEXTFIELD w="150" title="Device Imei 2" y="30" h="20" x="10">device_imei_2</TEXTFIELD>
					<CHECKBOX w="150" title="Active" y="30" h="20" x="290">is_active</CHECKBOX>
				</FORM>
			</GRID>
			<FORM name="Org" keyfield="org_id" table="orgs" noorg="true">
				<COMBOBOX w="430" title="Currency ID" lptable="currency" y="10" h="20" x="10" lpfield="currency_name">currency_id</COMBOBOX>
				<COMBOBOX w="430" title="Parent Org ID" lptable="orgs" lpkey="org_id" y="30" h="20" x="10" lpfield="org_name">parent_org_id</COMBOBOX>
				<TEXTFIELD w="150" title="Org Name" y="50" h="20" x="10">org_name</TEXTFIELD>
				<TEXTFIELD w="150" title="Org Sufix" y="50" h="20" x="290">org_sufix</TEXTFIELD>
				<CHECKBOX w="150" title="Default" y="70" h="20" x="10">is_default</CHECKBOX>
				<CHECKBOX w="150" title="Active" y="70" h="20" x="290">is_active</CHECKBOX>
				<TEXTFIELD w="150" title="Logo" y="90" h="20" x="10">logo</TEXTFIELD>
				<TEXTFIELD w="150" title="Pin" y="90" h="20" x="290">pin</TEXTFIELD>
				<TEXTAREA w="430" title="Details" y="110" h="70" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
	</DESK>
	<DESK w="700" name="Countys" noorg="true" key="10" h="550">
		<GRID name="Countys" keyfield="county_id" table="countys" noorg="true">
			<TEXTFIELD w="75" title="County ID">county_id</TEXTFIELD>
			<TEXTFIELD w="75" title="County Name">county_name</TEXTFIELD>
			<GRID name="Sub Countys" keyfield="sub_county_id" linkfield="county_id" table="vw_sub_countys" noorg="true">
				<TEXTFIELD w="75" title="County ID">county_id</TEXTFIELD>
				<TEXTFIELD w="75" title="County Name">county_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Sub County ID">sub_county_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Sub County Name">sub_county_name</TEXTFIELD>
				<FORM name="Sub Countys" keyfield="sub_county_id" linkfield="county_id" table="sub_countys" noorg="true">
					<TEXTFIELD w="150" title="Sub County Name" y="10" h="20" x="10">sub_county_name</TEXTFIELD>
				</FORM>
			</GRID>
			<FORM name="County" keyfield="county_id" table="countys" noorg="true">
				<TEXTFIELD w="150" title="County Name" y="10" h="20" x="10">county_name</TEXTFIELD>
			</FORM>
		</GRID>
	</DESK>
	<DESK w="700" name="Surveys" key="15" h="550">
		<GRID name="Countys" keyfield="county_id" table="countys" noorg="true">
			<TEXTFIELD w="75" title="County ID">county_id</TEXTFIELD>
			<TEXTFIELD w="75" title="County Name">county_name</TEXTFIELD>
			<GRID name="Sub Countys" keyfield="sub_county_id" linkfield="county_id" table="vw_sub_countys" noorg="true">
				<TEXTFIELD w="75" title="County ID">county_id</TEXTFIELD>
				<TEXTFIELD w="75" title="County Name">county_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Sub County ID">sub_county_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Sub County Name">sub_county_name</TEXTFIELD>
				<GRID name="Surveys" keyfield="survey_id" linkfield="sub_county_id" table="vw_surveys">
					<TEXTFIELD w="75" title="Survey ID">survey_id</TEXTFIELD>
					<TEXTFIELD w="75" title="Worker Name">worker_name</TEXTFIELD>
					<TEXTFIELD w="75" title="Org Name">org_name</TEXTFIELD>
					<TEXTFIELD w="75" title="County Name">county_name</TEXTFIELD>
					<TEXTFIELD w="75" title="Sub County Name">sub_county_name</TEXTFIELD>
					<TEXTFIELD w="75" title="Village Name">village_name</TEXTFIELD>
					<TEXTFIELD w="75" title="Household Number">household_number</TEXTFIELD>
					<TEXTFIELD w="75" title="Household Member">household_member</TEXTFIELD>
					<TEXTTIMESTAMP w="75" title="Survey Time">survey_time</TEXTTIMESTAMP>
					<TEXTFIELD w="75" title="Location Lat">location_lat</TEXTFIELD>
					<TEXTFIELD w="75" title="Location Lng">location_lng</TEXTFIELD>
					<GRID name="Mother Info" keyfield="survey_mother_id" linkfield="survey_id" table="vw_survey_mother" noorg="true">
						<TEXTFIELD w="75" title="Variable">mother_info_def_id</TEXTFIELD>
						<TEXTFIELD w="75" title="Question">question</TEXTFIELD>
						<TEXTFIELD w="75" title="Response">response</TEXTFIELD>
						<TEXTFIELD w="75" title="Response">response_name</TEXTFIELD>
						<FORM name="Mother Info" keyfield="survey_mother_id" linkfield="survey_id" table="survey_mother" noorg="true">
							<COMBOBOX w="430" title="Mother Info Def ID" lptable="mother_info_defs" noorg="true" y="10" h="20" x="10" lpfield="question">mother_info_def_id</COMBOBOX>
							<TEXTFIELD w="150" title="Response" y="30" h="20" x="10">response</TEXTFIELD>
						</FORM>
					</GRID>
					<GRID name="Child Info" keyfield="survey_child_id" linkfield="survey_id" table="vw_survey_child" noorg="true">
						<TEXTFIELD w="75" title="Variable">child_info_def_id</TEXTFIELD>
						<TEXTFIELD w="75" title="Question">question</TEXTFIELD>
						<TEXTFIELD w="75" title="Response">response</TEXTFIELD>
						<TEXTFIELD w="75" title="Response">response_name</TEXTFIELD>
						<FORM name="Child Info" keyfield="survey_child_id" linkfield="survey_id" table="survey_child" noorg="true">
							<COMBOBOX w="430" title="Child Info Def ID" lptable="child_info_defs" y="10" h="20" x="10" lpfield="question">child_info_def_id</COMBOBOX>
							<TEXTFIELD w="150" title="Response" y="30" h="20" x="10">response</TEXTFIELD>
						</FORM>
					</GRID>
					<GRID name="Referrals Info" keyfield="survey_referral_id" linkfield="survey_id" table="vw_survey_referrals" noorg="true">
						<TEXTFIELD w="75" title="Variable">referral_info_def_id</TEXTFIELD>
						<TEXTFIELD w="75" title="Question">question</TEXTFIELD>
						<TEXTFIELD w="75" title="Response">response</TEXTFIELD>
						<TEXTFIELD w="75" title="Response Name">response_name</TEXTFIELD>
						<FORM name="Survey Referrals" keyfield="survey_referral_id" linkfield="survey_id" table="survey_referrals" noorg="true">
							<COMBOBOX w="430" title="Referral Info Defs ID" lptable="referral_info_defs" lpkey="referral_info_def_id" y="10" h="20" x="10" lpfield="question">referral_info_defs_id</COMBOBOX>
							<TEXTFIELD w="430" title="Response" y="30" h="20" x="10">response</TEXTFIELD>
						</FORM>
					</GRID>
					<GRID name="Defaulters Info" keyfield="survey_defaulter_id" linkfield="survey_id" table="vw_survey_defaulters" noorg="true">
						<TEXTFIELD w="75" title="Variable">defaulters_info_def_id</TEXTFIELD>
						<TEXTFIELD w="75" title="Question">question</TEXTFIELD>
						<TEXTFIELD w="75" title="Response">response</TEXTFIELD>
						<TEXTFIELD w="75" title="Response">response_name</TEXTFIELD>
						<FORM name="Survey Defaulters" keyfield="survey_defaulter_id" linkfield="survey_id" table="survey_defaulters" noorg="true">
							<COMBOBOX w="430" title="Defaulters Info Def ID" lptable="defaulters_info_defs" y="10" h="20" x="10" lpfield="question">defaulters_info_def_id</COMBOBOX>
							<TEXTFIELD w="150" title="Response" y="30" h="20" x="10">response</TEXTFIELD>
						</FORM>
					</GRID>
					<GRID name="Household Info" keyfield="survey_household_id" linkfield="survey_id" table="vw_survey_household" noorg="true">
						<TEXTFIELD w="75" title="Variable">household_info_def_id</TEXTFIELD>
						<TEXTFIELD w="75" title="Question">question</TEXTFIELD>
						<TEXTFIELD w="75" title="Response">response</TEXTFIELD>
						<TEXTFIELD w="75" title="Response">response_name</TEXTFIELD>
						<FORM name="Survey Household" keyfield="survey_household_id" linkfield="survey_id" table="survey_household" noorg="true">
							<COMBOBOX w="430" title="Household Info Def ID" lptable="household_info_defs" y="10" h="20" x="10" lpfield="question">household_info_def_id</COMBOBOX>
							<TEXTFIELD w="150" title="Response" y="30" h="20" x="10">response</TEXTFIELD>
						</FORM>
					</GRID>
					<GRID name="Death Info" keyfield="survey_death_id" linkfield="survey_id" table="vw_survey_death" noorg="true">
						<TEXTFIELD w="75" title="Variable">death_info_def_id</TEXTFIELD>
						<TEXTFIELD w="75" title="Question">question</TEXTFIELD>
						<TEXTFIELD w="75" title="Death Count">response</TEXTFIELD>
						<FORM name="Survey Death" keyfield="survey_death_id" linkfield="survey_id" table="survey_death" noorg="true">
							<COMBOBOX w="430" title="Death Info Def ID" lptable="death_info_defs" y="10" h="20" x="10" lpfield="question">death_info_def_id</COMBOBOX>
							<TEXTFIELD w="430" title="Response" y="30" h="20" x="10">response</TEXTFIELD>
						</FORM>
					</GRID>
					<FORM name="Survey" keyfield="survey_id" linkfield="sub_county_id" table="surveys">
						<COMBOBOX w="430" title="Sub County ID" lptable="sub_countys" noorg="true" y="10" h="20" x="10" lpfield="sub_county_name">sub_county_id</COMBOBOX>
						<COMBOBOX w="430" title="Health Worker ID" lptable="health_workers" y="30" h="20" x="10" lpfield="worker_name">health_worker_id</COMBOBOX>
						<TEXTFIELD w="430" title="Village Name" y="50" h="20" x="10">village_name</TEXTFIELD>
						<TEXTFIELD w="150" title="Household Number" y="70" h="20" x="10">household_number</TEXTFIELD>
						<TEXTFIELD w="430" title="Household Member" y="90" h="20" x="10">household_member</TEXTFIELD>
						<TEXTFIELD w="150" title="Location Lat" y="110" h="20" x="290">location_lat</TEXTFIELD>
						<TEXTFIELD w="150" title="Location Lng" y="130" h="20" x="10">location_lng</TEXTFIELD>
						<TEXTAREA w="430" title="Remarks" y="150" h="70" x="10">remarks</TEXTAREA>
					</FORM>
				</GRID>
			</GRID>
		</GRID>
	</DESK>
</APP>
