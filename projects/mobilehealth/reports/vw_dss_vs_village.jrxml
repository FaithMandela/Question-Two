vw_survey_100.category, vw_survey_100.category_name
FROM vw_survey_100
WHERE vw_survey_100.village_id = '$P!{village_id}'
AND vw_survey_100.receiving_officer_name != ''
AND vw_survey_100.referral_time::date BETWEEN'$P!{start_date}' AND '$P!{end_date}'
