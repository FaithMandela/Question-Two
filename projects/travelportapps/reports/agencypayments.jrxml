	<queryString>
		<![CDATA[SELECT vw_payments.org_id, vw_payments.org_name, vw_payments.payment_type_id, vw_payments.payment_type_name, vw_payments.payment_id, SUM(vw_payments.payment_amount)AS amount, vw_payments.transaction_reference, vw_payments.payment_date, vw_payments.details, orgs.credit_limit, vw_passengers.cover_amount

FROM vw_payments
INNER JOIN orgs ON orgs.org_id = vw_payments.org_id
INNER JOIN vw_passengers ON vw_passengers.org_id = vw_payments.org_id
WHERE vw_payments.org_id = $P!{org_id}
GROUP BY vw_payments.org_id, vw_payments.org_name, vw_payments.payment_type_id, vw_payments.payment_type_name, vw_payments.payment_id, vw_payments.transaction_reference, vw_payments.payment_date, vw_payments.details,orgs.credit_limit,vw_passengers.cover_amount]]>
