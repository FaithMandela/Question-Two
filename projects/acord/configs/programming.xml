			<MENU name="Programmes Details">1</MENU>
		</MENU>
		<MENU name="Control Panel">
			<MENU name="Offices">6</MENU>
			<MENU name="Themes/Subthemes">7</MENU>
			<MENU name="Donors">8</MENU>
		</MENU>
		<MENU name="Reports Panel">
			<MENU name="Programmes">10</MENU>
			<MENU name="Activities">16</MENU>
			<MENU name="Themes">12</MENU>
			<MENU name="Donors">13</MENU>
			<MENU name="Contracts">14</MENU>
			<MENU name="Grants">15</MENU>
		</MENU>
		<MENU name="PDU" xml="acord.xml">0</MENU>
