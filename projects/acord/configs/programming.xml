	<DESK w="585" name="Programme Information" type="vert" key="1" h="500">
		<GRID name="Programmes" keyfield="programmeid" table="programmes" filterkey="4" key="1">
			<TEXTFIELD w="40" title="Programme Name">programmename</TEXTFIELD>
			<TEXTFIELD w="40" title="Location">location</TEXTFIELD>
			<TEXTFIELD w="40" title="Area Programme">areaprogramme</TEXTFIELD>
			<TEXTFIELD w="40" title="Thematic Programme">thematicprogramme</TEXTFIELD>
			<TEXTFIELD w="40" title="Pan-African Programme">panafricanprogramme</TEXTFIELD>
			<TEXTFIELD w="40" title="Start Date">starteddate</TEXTFIELD>
			<FORM name="Programmes" keyfield="programmeid" table="programmes" key="2">
				<TEXTFIELD w="130" title="Programme ID" y="10" h="20" x="10">programmeid</TEXTFIELD>
				<TEXTFIELD w="150" title="Programme Name" y="10" h="20" x="270">programmename</TEXTFIELD>
				<COMBOBOX w="130" title="Theme" lptable="themes" y="10" h="20" x="550" lpfield="themename">themeid</COMBOBOX>
				<COMBOBOX w="130" title="Country" lptable="sys_countrys" y="40" h="20" x="10" lpfield="sys_country_name">sys_country_id</COMBOBOX>
				<TEXTFIELD w="130" title="Location" y="70" h="20" x="10">location</TEXTFIELD>
				<TEXTFIELD w="130" title="Start Date" y="100" h="20" x="10">starteddate</TEXTFIELD>
				<TEXTAREA w="150" title="Address" y="40" h="80" x="270">address</TEXTAREA>
				<TEXTFIELD w="130" title="email" y="40" h="20" x="550">email</TEXTFIELD>
				<TEXTFIELD w="130" title="Telephone" y="70" h="20" x="550">telephone</TEXTFIELD>
				<TEXTFIELD w="130" title="Fax" y="100" h="20" x="550">fax</TEXTFIELD>
				<CHECKBOX w="130" title="Area Programme" y="130" h="20" x="10">areaprogramme</CHECKBOX>
				<CHECKBOX w="130" title="Thematic Programme" lw="160" y="130" h="20" x="270">thematicprogramme</CHECKBOX>
				<CHECKBOX w="130" title="Pan-African Programme" lw="160" y="130" h="20" x="550">panafricanprogramme</CHECKBOX>
				<TEXTFIELD w="130" title="Duration (years)" y="160" h="20" x="10">duration</TEXTFIELD>
				<TEXTDECIMAL w="150" title="Income" y="160" h="20" x="270">income</TEXTDECIMAL>
				<TEXTDECIMAL w="130" title="Budget" y="160" h="20" x="550">budget</TEXTDECIMAL>
				<TEXTFIELD w="40" title="Female Employees" y="190" h="20" x="10">femployees</TEXTFIELD>
				<TEXTFIELD w="40" title="Male Employees" y="190" h="20" x="270">memployees</TEXTFIELD>
				<TEXTAREA w="670" title="Programme Brief" y="220" h="80" x="10">programmebrief</TEXTAREA>
				<TEXTAREA w="670" title="Details" y="310" h="80" x="10">details</TEXTAREA>
				<FORM name="Projects" keyfield="projectid" table="projects" key="1">
					<TEXTFIELD w="130" title="Project ID" y="10" h="20" x="10">projectid</TEXTFIELD>
					<TEXTFIELD w="130" title="Project Name" y="10" h="20" x="270">projectname</TEXTFIELD>
					<COMBOBOX w="150" title="Programme" lptable="programmes" y="10" h="20" x="550" lpfield="programmename">programmeid</COMBOBOX>
					<COMBOBOX w="130" title="Country" lptable="sys_countrys" y="40" h="20" x="10" lpfield="sys_country_name">sys_country_id</COMBOBOX>
					<TEXTFIELD w="130" title="Location" y="40" h="20" x="270">location</TEXTFIELD>
					<TEXTFIELD w="150" title="Regional Desk" y="40" h="20" x="550">regionaldesk</TEXTFIELD>
					<TEXTAREA w="150" title="Address" y="70" h="80" x="10">address</TEXTAREA>
					<TEXTFIELD w="150" title="email" y="70" h="20" x="550">email</TEXTFIELD>
					<TEXTFIELD w="150" title="Telephone" y="100" h="20" x="550">telephone</TEXTFIELD>
					<TEXTFIELD w="150" title="Fax" y="130" h="20" x="550">fax</TEXTFIELD>
					<TEXTAREA w="250" title="History" y="160" h="80" x="10">history</TEXTAREA>
					<TEXTAREA w="250" title="Core" y="160" h="80" x="450">Core</TEXTAREA>
					<TEXTAREA w="250" title="Vision" y="250" h="80" x="10">vision</TEXTAREA>
					<TEXTAREA w="250" title="Objectives" y="250" h="80" x="450">objectives</TEXTAREA>
					<TEXTAREA w="690" title="Mission" y="340" h="80" x="10">mission</TEXTAREA>
					<TEXTAREA w="690" title="Details" y="430" h="60" x="10">details</TEXTAREA>
				</FORM>
				<GRID name="Contracts" keyfield="contractid" linkfield="projectid" table="contracts" key="3">
					<TEXTFIELD w="40" title="Contract ID">contractid</TEXTFIELD>
					<TEXTFIELD w="40" title="Project ID">projectid</TEXTFIELD>
					<TEXTFIELD w="40" title="Donor ID">donorid</TEXTFIELD>
					<TEXTFIELD w="40" title="Application Ref">applicationref</TEXTFIELD>
					<TEXTFIELD w="40" title="Currency">currency</TEXTFIELD>
					<TEXTFIELD w="40" title="Corefund">corefund</TEXTFIELD>
					<TEXTFIELD w="40" title="Contract Ref.">contractref</TEXTFIELD>
					<FORM name="Contracts" keyfield="contractid" table="contracts" key="1">
						<TEXTFIELD w="100" title="Contract ID" lw="100" y="10" h="20" x="10">contractid</TEXTFIELD>
						<COMBOBOX w="150" title="Project" lptable="projects" lw="100" y="10" h="20" x="250" lpfield="projectname">projectid</COMBOBOX>
						<COMBOBOX w="230" title="Donor" lptable="donors" lw="80" y="10" h="20" x="520" lpfield="donorname">donorid</COMBOBOX>
						<CHECKBOX w="130" title="Corefund" lw="80" y="40" h="20" x="520">corefund</CHECKBOX>
						<TEXTDATE w="130" title="Decision Date" lw="100" y="40" h="20" x="250">decisiondate</TEXTDATE>
						<TEXTDATE w="100" title="Contract Date" lw="100" y="40" h="20" x="10">contractdate</TEXTDATE>
						<TEXTDATE w="100" title="Start of Grant" lw="100" y="70" h="20" x="10">startofgrant</TEXTDATE>
						<TEXTDATE w="130" title="End of Grant" lw="100" y="70" h="20" x="250">endofgrant</TEXTDATE>
						<TEXTAREA w="730" title="Details" lw="100" y="100" h="70" x="10">details</TEXTAREA>
					</FORM>
					<GRID name="Deadlines" keyfield="deadlineid" linkfield="contractid" table="deadlineview" key="4">
						<TEXTFIELD w="40" title="Project Name">projectname</TEXTFIELD>
						<TEXTFIELD w="40" title="Contract Ref.">contractref</TEXTFIELD>
						<TEXTFIELD w="40" title="Event">event</TEXTFIELD>
						<TEXTFIELD w="40" title="Event Date">eventdate</TEXTFIELD>
						<TEXTFIELD w="40" title="Email">email</TEXTFIELD>
						<FORM name="Deadlines" keyfield="deadlineid" table="deadlines" key="2">
							<COMBOBOX w="200" title="Contract Ref." lptable="contracts" y="10" h="20" x="10" lpfield="contractref">contractid</COMBOBOX>
							<TEXTAREA w="200" title="Event" y="40" h="60" x="10">event</TEXTAREA>
							<TEXTDATE w="150" title="Event Date" y="40" h="20" x="350">eventdate</TEXTDATE>
							<TEXTFIELD w="150" title="Email" y="70" h="20" x="350">email</TEXTFIELD>
							<TEXTAREA w="490" title="Details" y="100" h="80" x="10">details</TEXTAREA>
						</FORM>
					</GRID>
				</GRID>
				<GRID name="Project Activities" keyfield="activityid" linkfield="projectid" table="activityview" key="5">
					<TEXTFIELD w="40" title="Project Name" lw="120">projectname</TEXTFIELD>
					<TEXTFIELD w="40" title="Sub Theme" lw="60">subthemename</TEXTFIELD>
					<TEXTFIELD w="40" title="Activity" lw="150">activity</TEXTFIELD>
					<TEXTFIELD w="40" title="Start-Date">startdate</TEXTFIELD>
					<TEXTFIELD w="40" title="Close-Date">closedate</TEXTFIELD>
					<TEXTFIELD w="40" title="Impact Rating" lw="60">impactrating</TEXTFIELD>
					<FORM name="Activities" keyfield="activityid" table="activities" key="3">
						<TEXTAREA w="300" title="Activity" lw="80" y="10" h="20" x="10">activity</TEXTAREA>
						<COMBOBOX w="200" title="Project Name" lptable="projects" lw="110" y="10" h="20" x="420" lpfield="projectname">projectid</COMBOBOX>
						<TEXTFIELD w="100" title="Start Date" lw="80" y="40" h="20" x="10">startdate</TEXTFIELD>
						<TEXTFIELD w="100" title="Close Date" lw="80" y="40" h="20" x="210">closedate</TEXTFIELD>
						<COMBOBOX w="200" title="Subtheme Name" lptable="subthemes" lw="110" y="40" h="20" x="420" lpfield="subthemename">subthemeid</COMBOBOX>
						<TEXTAREA w="640" title="Details" lw="80" y="70" h="60" x="10">details</TEXTAREA>
						<TEXTFIELD w="20" title="Impact Rating (1-5)" lw="130" y="140" h="20" x="90">impactrating</TEXTFIELD>
					</FORM>
				</GRID>
			</FORM>
			<GRID name="Projects" keyfield="projectid" linkfield="programmeid" table="projectview" key="2">
				<TEXTFIELD w="40" title="Project Name">projectname</TEXTFIELD>
				<TEXTFIELD w="40" title="Programme">programmename</TEXTFIELD>
				<TEXTFIELD w="40" title="Country">sys_country_name</TEXTFIELD>
				<TEXTFIELD w="40" title="Regional Desk">regionaldesk</TEXTFIELD>
			</GRID>
		</GRID>
		<FILTER name="Projects Filter" key="0">
			<COMBOBOX w="140" title="Programme" lptable="programmes" y="10" h="20" x="10" lpfield="programmename">programmeid</COMBOBOX>
		</FILTER>
	</DESK>
	<DESK w="595" name="Offices" type="vert" key="6" h="500">
		<GRID name="ACORD Offices" keyfield="officeid" table="officeview" filterkey="3" key="1">
			<TEXTFIELD w="40" title="Office ID">officeid</TEXTFIELD>
			<TEXTFIELD w="40" title="Description">officename</TEXTFIELD>
			<TEXTFIELD w="40" title="Country">sys_country_name</TEXTFIELD>
			<FORM name="Office" keyfield="officeid" table="offices" key="2">
				<TEXTFIELD w="100" title="Office ID" lw="80" y="10" h="20" x="10">officeid</TEXTFIELD>
				<TEXTFIELD w="250" title="Description" lw="80" y="10" h="20" x="220">officename</TEXTFIELD>
				<COMBOBOX w="150" title="Country" lptable="sys_countrys" lw="80" y="10" h="20" x="580" lpfield="sys_country_name">sys_country_id</COMBOBOX>
				<TEXTAREA w="720" title="Details" lw="80" y="40" h="60" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
	</DESK>
	<DESK w="585" name="Themes And Subthemes Management" type="vert" key="7" h="500">
		<GRID name="Themes" keyfield="themeid" table="themes" filterkey="2" key="1">
			<TEXTFIELD w="40" title="ThemeID">themeid</TEXTFIELD>
			<TEXTFIELD w="40" title="Theme Name">themename</TEXTFIELD>
			<TEXTFIELD w="40" title="Details">details</TEXTFIELD>
			<FORM name="Themes" keyfield="themeid" table="themes" key="3">
				<TEXTFIELD w="60" title="Theme ID" lw="80" y="10" h="20" x="10">themeid</TEXTFIELD>
				<TEXTFIELD w="230" title="Theme Name" lw="90" y="10" h="20" x="250">themename</TEXTFIELD>
				<TEXTAREA w="480" title="Details" lw="80" y="40" h="100" x="10">details</TEXTAREA>
			</FORM>
			<GRID name="Sub Themes" keyfield="subthemeid" linkfield="themeid" table="subthemes" key="2">
				<TEXTFIELD w="40" title="Subtheme">subthemename</TEXTFIELD>
				<TEXTFIELD w="40" title="Theme ID">themeid</TEXTFIELD>
				<TEXTFIELD w="40" title="Details">details</TEXTFIELD>
				<FORM name="Sub Themes" keyfield="subthemeid" table="subthemes" key="3">
					<TEXTFIELD w="80" title="Subtheme ID" lw="90" y="10" h="20" x="10">subthemeid</TEXTFIELD>
					<COMBOBOX w="190" title="Theme" lptable="themes" lw="80" y="10" h="20" x="240" lpfield="themename">themeid</COMBOBOX>
					<TEXTFIELD w="190" title="Subtheme" lw="80" y="10" h="20" x="540">subthemename</TEXTFIELD>
					<TEXTAREA w="710" title="Details" lw="90" y="40" h="100" x="10">details</TEXTAREA>
				</FORM>
			</GRID>
		</GRID>
	</DESK>
	<DESK w="585" name="Donor Management" type="vert" key="8" h="500">
		<GRID name="Donors" keyfield="donorid" table="donorview" filterkey="3" key="1">
			<TEXTFIELD w="40" title="Donor Name">donorname</TEXTFIELD>
			<TEXTFIELD w="40" title="Donor Group">donorgroupname</TEXTFIELD>
			<TEXTFIELD w="40" title="Local Donor">localdonor</TEXTFIELD>
			<TEXTFIELD w="40" title="Member Donor">memberdonor</TEXTFIELD>
			<TEXTFIELD w="40" title="email">email</TEXTFIELD>
			<FORM name="Donors" keyfield="donorid" table="donors" key="2">
				<TEXTFIELD w="130" title="Donor ID" lw="100" y="10" h="20" x="10">donorid</TEXTFIELD>
				<TEXTFIELD w="420" title="Donor Name" lw="100" y="10" h="20" x="280">donorname</TEXTFIELD>
				<COMBOBOX w="130" title="Donor Group" lptable="donorgroups" lw="100" y="40" h="20" x="10" lpfield="donorgroupname">donorgroupid</COMBOBOX>
				<COMBOBOX w="130" title="Country" lptable="sys_countrys" lw="100" y="40" h="20" x="280" lpfield="sys_country_name">sys_country_id</COMBOBOX>
				<TEXTFIELD w="150" title="Headquarters" lw="100" y="40" h="20" x="550">headquarters</TEXTFIELD>
				<TEXTFIELD w="130" title="Contact Person" lw="100" y="70" h="20" x="10">contactperson</TEXTFIELD>
				<CHECKBOX w="40" title="Local Donor" lw="100" y="100" h="20" x="10">localdonor</CHECKBOX>
				<CHECKBOX w="40" title="Member Donor" lw="100" y="130" h="20" x="10">memberdonor</CHECKBOX>
				<TEXTAREA w="130" title="Address" lw="100" y="70" h="80" x="280">address</TEXTAREA>
				<TEXTFIELD w="150" title="email" lw="100" y="70" h="20" x="550">email</TEXTFIELD>
				<TEXTFIELD w="150" title="Telephone" lw="100" y="100" h="20" x="550">telephone</TEXTFIELD>
				<TEXTFIELD w="150" title="Fax" lw="100" y="130" h="20" x="550">fax</TEXTFIELD>
				<TEXTAREA w="280" title="Donor Summary" lw="105" y="160" h="100" x="10">donorsummary</TEXTAREA>
				<TEXTAREA w="260" title="Funding Summary" y="160" h="100" x="420">fundingsummary</TEXTAREA>
				<TEXTAREA w="690" title="Details" lw="105" y="270" h="100" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
		<FILTER name="Donors Filter" key="3">
			<COMBOBOX w="140" title="Donor Group" lptable="donorgroups" y="40" h="20" x="10" lpfield="donorgroupname">donorgroupid</COMBOBOX>
		</FILTER>
	</DESK>
	<DESK w="700" name="Thematic Programme Report" type="horl" key="10" h="500">
		<FILTER location="250" name="Thematic Programme Report" type="horl">
			<JASPER filtered="false" name="Grouped Programme List" reportfile="/root/baraza/reports/acord/PDU/programmes_PDU_All.jasper"></JASPER>
			<DRILLDOWN name="Programmes Drill Down" keyfield="countrycodeid" table="programmecountryview" listfield="sys_country_name">
				<DRILLDOWN name="Programmes Drill Down" keyfield="programmeid" wherefield="countrycodeid" table="programmeview" listfield="programmename" pos="top"></DRILLDOWN>
			</DRILLDOWN>
			<JASPER filtered="true" name="Geographical Area Programmes" reportfile="/root/baraza/reports/acord/PDU/programmes_PDU.jasper"></JASPER>
			<JASPER filtered="true" name="Projects" reportfile="/root/baraza/reports/acord/PDU/projects_PDU_All.jasper"></JASPER>
		</FILTER>
	</DESK>
	<DESK w="700" name="Activities Report" type="horl" key="16" h="500">
		<FILTER location="250" name="Activities Report" type="horl">
			<JASPER filtered="false" name="All Projects Activities" reportfile="/root/baraza/reports/acord/programming/project_activities.jasper"></JASPER>
			<DRILLDOWN name="Activities Drill Down" keyfield="themeid" table="themes" listfield="themename">
				<DRILLDOWN name="Subthemes" keyfield="subthemeid" wherefield="themeid" table="subthemes" listfield="subthemename" pos="top"></DRILLDOWN>
			</DRILLDOWN>
			<JASPER filtered="true" name="Thematic Activities" reportfile="/root/baraza/reports/acord/programming/thematic_activities.jasper"></JASPER>
		</FILTER>
	</DESK>
	<DESK w="700" name="Themes Report" type="horl" key="12" h="500">
		<FILTER location="250" name="Themes Report" type="horl">
			<JASPER filtered="false" name="Themes Listing" reportfile="/root/baraza/reports/acord/themes.jasper"></JASPER>
			<JASPER filtered="false" name="Subthemes List" reportfile="/root/baraza/reports/acord/subthemes_all.jasper"></JASPER>
			<DRILLDOWN name="Themes Drilldown" keyfield="themeid" table="Themes" listfield="Themename">
				<DRILLDOWN name="Subthemes" keyfield="subthemeid" wherefield="themeid" table="subthemes" listfield="subthemename" pos="top"></DRILLDOWN>
			</DRILLDOWN>
			<JASPER filtered="true" name="Subthemes Drilldown" reportfile="/root/baraza/reports/acord/subthemes.jasper"></JASPER>
		</FILTER>
	</DESK>
	<DESK w="700" name="Donor Report" type="horl" key="13" h="500">
		<FILTER location="250" name="Donor Report" type="horl">
			<JASPER filtered="false" name="Donors List" reportfile="/root/baraza/reports/acord/PDU/donors_PDU_All.jasper"></JASPER>
			<DRILLDOWN name="Donors Drill Down" keyfield="donorid" table="donors" listfield="donorname"></DRILLDOWN>
			<JASPER filtered="true" name="Donor Summary" reportfile="/root/baraza/reports/acord/PDU/donor_summary.jasper"></JASPER>
			<JASPER filtered="true" name="Donor Funding Summary" reportfile="/root/baraza/reports/acord/PDU/donor_fundingsummary.jasper"></JASPER>
		</FILTER>
	</DESK>
	<DESK w="700" name="Contract Report" type="horl" key="14" h="500">
		<FILTER location="250" name="Contract Report" type="horl">
			<JASPER filtered="false" name="Grouped Contracts List" reportfile="/root/baraza/reports/acord/finance/contract_fin_all.jasper"></JASPER>
			<DRILLDOWN name="Contracts Drill Down" keyfield="programmeid" table="programmes" listfield="programmename">
				<DRILLDOWN name="Contracts" keyfield="projectid" wherefield="programmeid" table="projects" listfield="projectname" pos="top"></DRILLDOWN>
			</DRILLDOWN>
			<JASPER filtered="true" name="Contracts Drilldown" reportfile="/root/baraza/reports/acord/finance/contract_fin.jasper"></JASPER>
		</FILTER>
	</DESK>
	<DESK w="700" name="Grant Reports" type="horl" key="15" h="500">
		<FILTER location="250" name="Grant Reports" type="horl">
			<JASPER filtered="false" name="General Banking Details" reportfile="/root/baraza/reports/acord/finance/banking_all.jasper"></JASPER>
			<DRILLDOWN name="Grants Drill Down" keyfield="projectid" table="projects" listfield="projectname">
				<DRILLDOWN name="Banking" keyfield="contractid" wherefield="projectid" table="contracts" listfield="contractid" pos="top"></DRILLDOWN>
			</DRILLDOWN>
			<JASPER filtered="true" name="Grants" reportfile="/root/baraza/reports/acord/finance/grants.jasper"></JASPER>
			<JASPER filtered="true" name="Banking Details" reportfile="/root/baraza/reports/acord/finance/banking.jasper"></JASPER>
		</FILTER>
	</DESK>
</APP>
