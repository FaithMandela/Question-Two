<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="comparison2" language="groovy" pageWidth="842" pageHeight="595" orientation="Landscape" columnWidth="802" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20">
	<queryString>
		<![CDATA[SELECT branches.branchname, categories.gender, count(teams.teamid)
FROM branches
INNER JOIN teams ON teams.branchid = branches.branchid
INNER JOIN categories ON teams.categoryid = categories.categoryid
WHERE categories.gender <> 'M'
GROUP BY branches.branchname, categories.gender]]>
	</queryString>
