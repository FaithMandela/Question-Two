						<TEXTAREA w="430" title="Details" y="410" h="70" x="10">details</TEXTAREA>
					</FORM>
				</GRID>
			</GRID>
		</GRID>
		<GRID name="Sessions" keyfield="session_id" table="vw_sessions">
