					<TEXTFIELD w="75" title="G Zipcode">g_zipcode</TEXTFIELD>
					<TEXTFIELD w="75" title="G Town">g_town</TEXTFIELD>
					<TEXTFIELD w="75" title="Sys Country ID">sys_country_id</TEXTFIELD>
					<TEXTFIELD w="75" title="G Telno">g_telno</TEXTFIELD>
					<TEXTFIELD w="75" title="G Email">g_email</TEXTFIELD>
