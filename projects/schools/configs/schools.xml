<APP color="green" name="Schools" org="org_id" password="change_password" material="true">
	<MENU name="School" background="red">
		<MENU name="Students">1</MENU>
		<MENU name="Timetable">5</MENU>
	</MENU>
	<DESK w="700" name="Students" key="1" h="550">
		<GRID name="Students" keyfield="student_id" table="vw_students">
			<TEXTFIELD w="75" title="Student Name">student_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Student ID">student_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Class ID">class_id</TEXTFIELD>
			<CHECKBOX w="75" title="Sex">sex</CHECKBOX>
			<TEXTFIELD w="75" title="Nationality">nationality</TEXTFIELD>
			<TEXTDATE w="75" title="Birth Date">birth_date</TEXTDATE>
			<TEXTFIELD w="75" title="Address">address</TEXTFIELD>
			<TEXTFIELD w="75" title="Zipcode">zipcode</TEXTFIELD>
			<TEXTFIELD w="75" title="Town">town</TEXTFIELD>
			<TEXTFIELD w="75" title="Country Code ID">country_code_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Telno">telno</TEXTFIELD>
			<TEXTFIELD w="75" title="Email">email</TEXTFIELD>
			<TEXTFIELD w="75" title="Fathers Name">fathers_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Fathers Tel No">fathers_tel_no</TEXTFIELD>
			<TEXTFIELD w="75" title="Fathers Email">fathers_email</TEXTFIELD>
			<TEXTFIELD w="75" title="Mothers Name">mothers_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Mothers Tel No">mothers_tel_no</TEXTFIELD>
			<TEXTFIELD w="75" title="Mothers Email">mothers_email</TEXTFIELD>
			<TEXTFIELD w="75" title="Guardian Name">guardian_name</TEXTFIELD>
			<FORM cols="2" tw="570" name="Students" keyfield="student_id" table="students" th="180">
				<COMBOBOX w="430" title="Class ID" lptable="stream_classes" lpkey="stream_class_id" y="30" h="20" x="10" span="true" lpfield="stream_class_id">class_id</COMBOBOX>
				<TEXTFIELD w="150" title="Student Name" y="50" h="20" x="290">student_name</TEXTFIELD>
				<COMBOLIST w="150" title="Gender" y="50" ischar="true" h="20" x="10" span="true">sex
					<DATA>M</DATA>
					<DATA>F</DATA>
				</COMBOLIST>
				<COMBOBOX w="430" title="Nationality" lptable="sys_countrys" lpkey="sys_country_id" y="70" h="20" x="290" lpfield="sys_country_name">sys_country_name</COMBOBOX>
				<TEXTDATE w="150" title="Birth Date" y="90" h="20" x="10" span="true">birth_date</TEXTDATE>
				<TEXTFIELD w="150" title="Address" y="90" h="20" x="290">address</TEXTFIELD>
				<TEXTFIELD w="150" title="Zipcode" y="110" h="20" x="10" span="true">zipcode</TEXTFIELD>
				<TEXTFIELD w="150" title="Town" y="110" h="20" x="290">town</TEXTFIELD>
				<TEXTFIELD w="150" title="Telephone Number" y="150" h="20" x="10" span="true">telno</TEXTFIELD>
				<TEXTFIELD w="430" title="Email" y="170" h="20" x="290">email</TEXTFIELD>
				<TEXTFIELD w="430" title="Fathers Name" y="190" h="20" x="10" span="true">fathers_name</TEXTFIELD>
				<TEXTFIELD w="150" title="Fathers Tel No" y="210" h="20" x="290">fathers_tel_no</TEXTFIELD>
				<TEXTFIELD w="430" title="Fathers Email" y="230" h="20" x="10" span="true">fathers_email</TEXTFIELD>
				<TEXTFIELD w="430" title="Mothers Name" y="250" h="20" x="290">mothers_name</TEXTFIELD>
				<TEXTFIELD w="150" title="Mothers Tel No" y="270" h="20" x="10" span="true">mothers_tel_no</TEXTFIELD>
				<TEXTFIELD w="430" title="Mothers Email" y="290" h="20" x="290">mothers_email</TEXTFIELD>
				<TEXTFIELD w="150" title="Guardian Name" y="310" h="20" x="10" span="true">guardian_name</TEXTFIELD>
				<TEXTFIELD w="150" title="G Address" y="310" h="20" x="290">g_address</TEXTFIELD>
				<TEXTFIELD w="150" title="G Zipcode" y="330" h="20" x="10" span="true">g_zipcode</TEXTFIELD>
				<TEXTFIELD w="150" title="G Town" y="330" h="20" x="290">g_town</TEXTFIELD>
				<COMBOBOX w="430" title="G Countrycodeid" lptable="sys_countrys" lpkey="sys_country_id" y="350" h="20" x="10" span="true" lpfield="sys_country_name">g_countrycodeid</COMBOBOX>
				<TEXTFIELD w="150" title="G Telno" y="370" h="20" x="290">g_telno</TEXTFIELD>
				<TEXTFIELD w="430" title="G Email" y="390" h="20" x="10" span="true">g_email</TEXTFIELD>
				<TEXTAREA w="430" title="Current Contact" y="410" h="70" x="290">current_contact</TEXTAREA>
				<TEXTAREA w="430" title="Registrar Details" y="480" h="70" x="10" span="true">registrar_details</TEXTAREA>
				<TEXTAREA w="430" title="Details" y="550" h="70" x="290">details</TEXTAREA>
			</FORM>
		</GRID>
		<GRID name="Stream Classes" keyfield="stream_class_id" linkfield="org_id" table="vw_stream_classes">
			<TEXTFIELD w="75" title="Stream Class ID">stream_class_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Class Level">class_level</TEXTFIELD>
			<TEXTFIELD w="75" title="Stream">stream</TEXTFIELD>
			<TEXTFIELD w="75" title="Narrative">narrative</TEXTFIELD>
			<FORM name="Stream Classes" keyfield="stream_class_id" linkfield="org_id" table="stream_classes">
				<TEXTFIELD w="150" title="Class Level" y="10" h="20" x="10">class_level</TEXTFIELD>
				<TEXTFIELD w="150" title="Stream" y="10" h="20" x="290">stream</TEXTFIELD>
				<TEXTFIELD w="430" title="Narrative" y="30" h="20" x="10">narrative</TEXTFIELD>
				<TEXTAREA w="430" title="Details" y="50" h="70" x="10">details</TEXTAREA>
			</FORM>
			<GRID name="Sessions" keyfield="session_id" linkfield="org_id" table="vw_sessions">
				<TEXTFIELD w="75" title="Session ID">session_id</TEXTFIELD>
				<TEXTFIELD w="75" title="Session Name">session_name</TEXTFIELD>
				<TEXTDATE w="75" title="Session Start Date">session_start_date</TEXTDATE>
				<TEXTDATE w="75" title="Session End Date">session_end_date</TEXTDATE>
				<FORM name="Sessions" keyfield="session_id" linkfield="org_id" table="sessions">
					<TEXTFIELD w="150" title="Session Name" y="10" h="20" x="10">session_name</TEXTFIELD>
					<TEXTDATE w="150" title="Session Start Date" y="10" h="20" x="290">session_start_date</TEXTDATE>
					<TEXTDATE w="150" title="Session End Date" y="30" h="20" x="10">session_end_date</TEXTDATE>
					<TEXTAREA w="430" title="Details" y="50" h="70" x="10">details</TEXTAREA>
				</FORM>
				<GRID name="Exams" keyfield="exam_id" linkfield="session_id" table="vw_exams">
					<TEXTFIELD w="75" title="Exam ID">exam_id</TEXTFIELD>
					<TEXTFIELD w="75" title="Class Level">class_level</TEXTFIELD>
					<TEXTFIELD w="75" title="Exam File">exam_file</TEXTFIELD>
					<FORM name="Exams" keyfield="exam_id" linkfield="session_id" table="exams">
						<COMBOBOX w="430" title="Subject ID" lptable="subjects" y="10" h="20" x="10" lpfield="subject_name">subject_id</COMBOBOX>
						<COMBOBOX w="430" title="Class Level" lptable="stream_classes" lpkey="stream_class_id" y="30" h="20" x="10" lpfield="stream_class_id">class_level</COMBOBOX>
						<TEXTFIELD w="150" title="Exam File" y="50" h="20" x="10">exam_file</TEXTFIELD>
						<TEXTAREA w="430" title="Exam Narrative" y="70" h="70" x="10">exam_narrative</TEXTAREA>
					</FORM>
				</GRID>
			</GRID>
		</GRID>
	</DESK>
	<DESK w="700" name="Timetable" key="5" h="550">
		<GRID name="Timetable" keyfield="timetable_id" table="vw_timetable">
			<TEXTFIELD w="75" title="Session ID">session_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Session Name">session_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Staff ID">staff_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Surname">surname</TEXTFIELD>
			<TEXTFIELD w="75" title="Stream Class ID">stream_class_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Stream">stream</TEXTFIELD>
			<TEXTFIELD w="75" title="Org ID">org_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Subject ID">subject_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Subject Name">subject_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Timetable ID">timetable_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Class ID">class_id</TEXTFIELD>
			<CHECKBOX w="75" title="Monday">monday</CHECKBOX>
			<CHECKBOX w="75" title="Tuesday">tuesday</CHECKBOX>
			<CHECKBOX w="75" title="Wednesday">wednesday</CHECKBOX>
			<CHECKBOX w="75" title="Thursday">thursday</CHECKBOX>
			<CHECKBOX w="75" title="Friday">friday</CHECKBOX>
			<CHECKBOX w="75" title="Saturday">saturday</CHECKBOX>
			<SPINTIME w="75" title="Start Time">start_time</SPINTIME>
			<SPINTIME w="75" title="End Time">end_time</SPINTIME>
			<FORM name="Timetable" keyfield="timetable_id" table="timetable">
				<COMBOBOX w="430" title="Class ID" lptable="stream_classes" lpkey="stream_class_id" h="20" y="10" lpfield="stream_class_id" x="10">class_id</COMBOBOX>
				<COMBOBOX w="430" title="Session ID" lptable="sessions" h="20" y="30" lpfield="session_name" x="10">session_id</COMBOBOX>
				<COMBOBOX w="430" title="Subject ID" lptable="subjects" h="20" y="50" lpfield="subject_name" x="10">subject_id</COMBOBOX>
				<COMBOBOX w="430" title="Staff ID" lptable="staff" h="20" y="70" lpfield="staff_id" x="10">staff_id</COMBOBOX>
				<CHECKBOX w="150" title="Monday" h="20" y="90" x="10">monday</CHECKBOX>
				<CHECKBOX w="150" title="Tuesday" h="20" y="90" x="290">tuesday</CHECKBOX>
				<CHECKBOX w="150" title="Wednesday" h="20" y="110" x="10">wednesday</CHECKBOX>
				<CHECKBOX w="150" title="Thursday" h="20" y="110" x="290">thursday</CHECKBOX>
				<CHECKBOX w="150" title="Friday" h="20" y="130" x="10">friday</CHECKBOX>
				<CHECKBOX w="150" title="Saturday" h="20" y="130" x="290">saturday</CHECKBOX>
				<SPINTIME w="150" title="Start Time" h="20" y="150" x="10">start_time</SPINTIME>
				<SPINTIME w="150" title="End Time" h="20" y="150" x="290">end_time</SPINTIME>
				<TEXTAREA w="430" title="Narrative" h="70" y="170" x="10">narrative</TEXTAREA>
			</FORM>
		</GRID>
	</DESK>
</APP>
