<APP>
	<MENU name="Sacco">
		<MENU name="Member">
			<MENU name="My Details">23</MENU>
			<MENU name="My Loan">24</MENU>
		</MENU>
		<MENU name="Entries">
			<MENU name="Members">1</MENU>
			<MENU name="Loans">2</MENU>
			<MENU name="Payments">3</MENU>
		</MENU>
		<MENU name="Reports">
			<MENU name="Members">11</MENU>
			<MENU name="Loan Types">12</MENU>
			<MENU name="Loans">14</MENU>
			<MENU name="Member Payment">15</MENU>
			<MENU name="Monthly Payments">16</MENU>
			<MENU name="Year Summary">17</MENU>
		</MENU>
	</MENU>
	<DESK w="700" name="Members" type="vert" key="1" h="580">
		<GRID name="Members" keyfield="memberid" table="members">
			<TEXTFIELD w="40" title="Member Name">membername</TEXTFIELD>
			<TEXTFIELD w="40" title="Staff Number">staffno</TEXTFIELD>
			<TEXTFIELD w="40" title="Mobile">mobile</TEXTFIELD>
			<TEXTFIELD w="40" title="Isactive">isactive</TEXTFIELD>
			<FORM name="Members" keyfield="memberid" table="members">
				<TEXTFIELD w="150" title="Member Name" y="30" h="20" x="10">membername</TEXTFIELD>
				<TEXTFIELD w="150" title="Staff Number" y="50" h="20" x="290">staffno</TEXTFIELD>
				<TEXTFIELD w="150" title="ID Number" y="30" h="20" x="290">idnumber</TEXTFIELD>
				<TEXTFIELD w="150" title="Address" y="70" h="20" x="290">address</TEXTFIELD>
				<TEXTFIELD w="150" title="Zip Code" y="50" h="20" x="10">zipcode</TEXTFIELD>
				<TEXTFIELD w="150" title="Town" y="90" h="20" x="290">town</TEXTFIELD>
				<TEXTFIELD w="150" title="Country" y="70" h="20" x="10">country</TEXTFIELD>
				<TEXTFIELD w="150" title="Tel Number" y="110" h="20" x="290">telno</TEXTFIELD>
				<TEXTFIELD w="150" title="Mobile" y="90" h="20" x="10">mobile</TEXTFIELD>
				<TEXTFIELD w="150" title="Email" y="110" h="20" x="10">email</TEXTFIELD>
				<TEXTDATE w="150" title="Entry Date" y="130" h="20" x="10">entrydate</TEXTDATE>
				<TEXTDECIMAL w="150" title="Entry Amount" y="130" h="20" x="290">entryamount</TEXTDECIMAL>
				<TEXTDATE w="150" title="Start Date" y="150" h="20" x="10">startdate</TEXTDATE>
				<TEXTDECIMAL w="150" title="Payroll" y="150" h="20" x="290">payroll</TEXTDECIMAL>
				<TEXTFIELD w="150" title="Member Login" y="170" h="20" x="10">memberlogin</TEXTFIELD>
				<CHECKBOX w="150" title="Active" y="170" h="20" x="290">isactive</CHECKBOX>
				<TEXTDATE w="150" title="Exit Date" y="190" h="20" x="10">exitdate</TEXTDATE>
				<TEXTDECIMAL w="150" title="Exit Amount" y="190" h="20" x="290">exitamount</TEXTDECIMAL>
				<TEXTAREA w="430" title="Details" y="210" h="70" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
	</DESK>
	<DESK w="700" name="Loans" type="vert" key="2" h="500">
		<GRID name="Loans" keyfield="loanid" table="loanview">
			<TEXTFIELD w="40" title="Member ">membername</TEXTFIELD>
			<TEXTFIELD w="40" title="LoanType ">loantypename</TEXTFIELD>
			<TEXTFIELD w="40" title="principle">principle</TEXTFIELD>
			<TEXTFIELD w="40" title="Repayment Period">repaymentperiod</TEXTFIELD>
			<TEXTFIELD w="40" title=" Period">calcrepaymentperiod</TEXTFIELD>
			<FORM name="Loans" keyfield="loanid" table="loans" key="2">
				<COMBOBOX w="150" title="LoanType " lptable="loantypes" y="10" h="20" x="10" lpfield="loantypename">loantypeid</COMBOBOX>
				<COMBOBOX w="150" title="Member " lptable="members" y="10" h="20" x="290" lpfield="membername">memberid</COMBOBOX>
				<TEXTDATE w="150" title="Loan Date" y="30" h="20" x="10">loandate</TEXTDATE>
				<TEXTDECIMAL w="150" title="Principle" y="30" h="20" x="290">principle</TEXTDECIMAL>
				<TEXTDECIMAL w="150" title="Interest" y="50" h="20" x="10">interest</TEXTDECIMAL>
				<TEXTFIELD w="150" title="Repayment Period" y="50" h="20" x="290">repaymentperiod</TEXTFIELD>
				<TEXTDECIMAL w="150" title="Monthly Repayment" y="70" h="20" x="10">monthlyrepayment</TEXTDECIMAL>
				<CHECKBOX w="150" title="Approval" y="70" h="20" x="290">loanapproved</CHECKBOX>
				<TEXTAREA w="430" title="Details" y="90" h="70" x="10">details</TEXTAREA>
			</FORM>
			<JASPER filtered="true" name="Payment Report" keyfield="loanid" reportfile="loanpaymentview.jasper" key="5"></JASPER>
			<GRID name="Gurantor" keyfield="gurantorid" linkfield="loanid" table="gurantorview" key="6">
				<TEXTFIELD w="40" title="Member">membername</TEXTFIELD>
				<TEXTFIELD w="40" title="Gurant amount">amount</TEXTFIELD>
				<FORM name="Gurantor" keyfield="gurantorid" linkfield="loanid" table="gurantor" key="7" inputkey="1">
					<COMBOBOX w="430" title="Member " lptable="members" y="10" h="20" x="10" lpfield="membername">memberid</COMBOBOX>
					<TEXTDECIMAL w="150" title="Guarantor Amount" y="30" h="20" x="10">amount</TEXTDECIMAL>
					<TEXTAREA w="430" title="Details" y="50" h="70" x="10">details</TEXTAREA>
				</FORM>
			</GRID>
		</GRID>
		<GRID name="Loan Types" keyfield="loantypeid" table="loantypes" key="3">
			<TEXTFIELD w="40" title="Loantype Name">loantypename</TEXTFIELD>
			<TEXTFIELD w="40" title="Interest">defaultinterest</TEXTFIELD>
			<FORM name="Loan Types" keyfield="loantypeid" table="loantypes" key="4">
				<TEXTFIELD w="150" title="Type Name" y="10" h="20" x="10">loantypename</TEXTFIELD>
				<TEXTFIELD w="150" title="Interest" y="10" h="20" x="290">defaultinterest</TEXTFIELD>
				<TEXTAREA w="430" title="Details" y="30" h="70" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
	</DESK>
	<DESK w="700" name="Payments" type="vert" key="3" h="500">
		<GRID name="Periods" keyfield="periodid" table="periods" key="1">
			<TEXTFIELD w="40" title="Start Date">startdate</TEXTFIELD>
			<TEXTFIELD w="40" title="End Date">enddate</TEXTFIELD>
			<TEXTFIELD w="40" title="Divided Rate">dividedrate</TEXTFIELD>
			<TEXTFIELD w="40" title="Closemonth">closemonth</TEXTFIELD>
			<FORM name="Periods" keyfield="periodid" table="periods" key="2">
				<TEXTDATE w="150" title="Start Date" y="10" h="20" x="10">startdate</TEXTDATE>
				<TEXTDATE w="150" title="End Date" y="10" h="20" x="290">enddate</TEXTDATE>
				<TEXTDECIMAL w="150" title="Divided Rate" y="30" h="20" x="10">dividedrate</TEXTDECIMAL>
				<CHECKBOX w="150" title="CloseMonth" y="30" h="20" x="290">closemonth</CHECKBOX>
				<TEXTAREA w="430" title="Details" y="50" h="70" x="10">details</TEXTAREA>
			</FORM>
			<GRID name="Loan Monthly" keyfield="loanmonthid" linkfield="periodid" table="loanmonthlyview" key="3">
				<TEXTFIELD w="40" title="Name ">membername</TEXTFIELD>
				<TEXTFIELD w="40" title="Period ">startdate</TEXTFIELD>
				<TEXTFIELD w="40" title="Loan ">principle</TEXTFIELD>
				<TEXTFIELD w="40" title="InterestAmount">interestamount</TEXTFIELD>
				<TEXTFIELD w="40" title="InterestPaid ">interestpaid</TEXTFIELD>
				<FORM name="Loan Monthly" keyfield="loanmonthid" linkfield="periodid" table="loanmonthly" key="4" inputkey="1">
					<GRIDBOX w="430" title="Loan " lptable="loanview" y="10" h="20" x="10" lpfield="(loantypename || ', ' || membername || ', ' || principle)">loanid
						<GRID name="Loans" keyfield="loanid" gridfilter="true" table="loanview" key="1" pos="top">
							<TEXTFIELD w="40" title="Member ">membername</TEXTFIELD>
							<TEXTFIELD w="40" title="LoanType ">loantypename</TEXTFIELD>
							<TEXTFIELD w="40" title="principle">principle</TEXTFIELD>
							<TEXTFIELD w="40" title="Repayment Period">repaymentperiod</TEXTFIELD>
							<TEXTFIELD w="40" title=" Period">calcrepaymentperiod</TEXTFIELD>
						</GRID>
					</GRIDBOX>
					<TEXTDECIMAL w="150" title="Interest Amount" y="30" h="20" x="10">interestamount</TEXTDECIMAL>
					<TEXTDECIMAL w="150" title="Interest Paid " y="30" h="20" x="290">interestpaid</TEXTDECIMAL>
					<TEXTDECIMAL w="150" title="Repayment" y="50" h="20" x="10">repayment</TEXTDECIMAL>
					<TEXTAREA w="430" title="Details" y="70" h="70" x="10">details</TEXTAREA>
				</FORM>
			</GRID>
			<GRID name="Member Monthly" keyfield="membermonthid" linkfield="periodid" table="membermonthlyview" key="5">
				<TEXTFIELD w="40" title="Member ">membername</TEXTFIELD>
				<TEXTFIELD w="40" title="Period">startdate</TEXTFIELD>
				<TEXTFIELD w="40" title="Payroll">payroll</TEXTFIELD>
				<TEXTFIELD w="40" title="Contribution">contribution</TEXTFIELD>
				<FORM name="Member Monthly" keyfield="membermonthid" table="membermonthly" key="6">
					<COMBOBOX w="350" title="Member " lptable="members" y="10" h="20" x="10" lpfield="membername">memberid</COMBOBOX>
					<TEXTDECIMAL w="150" title="Payroll" y="30" h="20" x="10">payroll</TEXTDECIMAL>
					<TEXTDECIMAL w="150" title="Add Funds" y="30" h="20" x="290">addfunds</TEXTDECIMAL>
					<TEXTDECIMAL w="150" title="Contribution" y="50" h="20" x="10">contribution</TEXTDECIMAL>
					<TEXTDECIMAL w="150" title="Divided" y="50" h="20" x="290">divided</TEXTDECIMAL>
					<TEXTFIELD w="430" title="Narrative" y="70" h="20" x="10">narrative</TEXTFIELD>
				</FORM>
			</GRID>
			<ACTION statement="updPeriods" name="Update member records" keyfield="periodid" key="7">
				<ACTION statement="updPeriods" title="Update member records">periodid</ACTION>
			</ACTION>
		</GRID>
	</DESK>
	<DESK w="700" name="Members" type="horl" key="11" h="500">
		<JASPER name="Active Members" reportfile="membersa.jasper"></JASPER>
		<JASPER name="In Active Members" reportfile="membersb.jasper"></JASPER>
		<JASPER name="All Members" reportfile="members.jasper"></JASPER>
	</DESK>
	<DESK w="700" name="Loan Types" type="horl" key="12" h="500">
		<JASPER name="Loans Report" reportfile="loantypes.jasper"></JASPER>
	</DESK>
	<DESK w="700" name="Loan Report" type="horl" key="14" h="500">
		<JASPER name="Appoved Loans" reportfile="loanview.jasper"></JASPER>
		<JASPER name="Approved and Open Loans" reportfile="loanviewa.jasper"></JASPER>
		<JASPER name="Closed Loan" reportfile="loanviewb.jasper"></JASPER>
		<JASPER name="Un Approved  Loan" reportfile="loanviewc.jasper"></JASPER>
	</DESK>
	<DESK w="700" name="Member Payments" type="horl" key="15" h="500">
		<FILTER location="250" name="Member Payments" type="horl">
			<DRILLDOWN name="Drill" keyfield="memberid" table="members" listfield="membername"></DRILLDOWN>
			<JASPER filtered="true" name="Contibution" reportfile="membermonthlyview.jasper"></JASPER>
			<JASPER filtered="true" name="Loan Payment" reportfile="loanmonthlyview.jasper"></JASPER>
		</FILTER>
	</DESK>
	<DESK w="700" name="Monthly Report" type="horl" key="16" h="500">
		<FILTER location="250" name="Monthly Report" type="horl">
			<DRILLDOWN name="Year" keyfield="PeriodYear" table="PeriodYearView" listfield="PeriodYear">
				<DRILLDOWN name="Month" keyfield="PeriodID" orderby="startdate" wherefield="PeriodYear" table="PeriodView" listfield="PeriodMonth" pos="top"></DRILLDOWN>
			</DRILLDOWN>
			<JASPER filtered="true" name="Contibution" reportfile="membermonthlyviewa.jasper"></JASPER>
			<JASPER filtered="true" name="Loan Payment" reportfile="loanmonthlyviewa.jasper"></JASPER>
		</FILTER>
	</DESK>
	<DESK w="700" name="Year Summary" type="horl" key="17" h="500">
		<FILTER location="250" name="Year Summary" type="horl">
			<DRILLDOWN name="Year" keyfield="PeriodYear" table="PeriodYearView" listfield="PeriodYear"></DRILLDOWN>
			<JASPER filtered="true" name="Year Summary" reportfile="periodsummary.jasper"></JASPER>
		</FILTER>
	</DESK>
	<DESK w="700" name="Members" key="23" h="500">
		<GRID name="Members" keyfield="memberid" display="form" table="members" user="memberid">
			<TEXTFIELD w="15" title="Member ID">memberid</TEXTFIELD>
			<TEXTFIELD w="15" title="member Name">membername</TEXTFIELD>
			<TEXTFIELD w="15" title="Staff No">staffno</TEXTFIELD>
			<TEXTFIELD w="15" title="ID Number">idnumber</TEXTFIELD>
			<TEXTFIELD w="15" title="Address">address</TEXTFIELD>
			<TEXTFIELD w="15" title="Zip Code">zipcode</TEXTFIELD>
			<TEXTFIELD w="15" title="Town">town</TEXTFIELD>
			<TEXTFIELD w="15" title="Country">country</TEXTFIELD>
			<TEXTFIELD w="15" title="Tel. No.">telno</TEXTFIELD>
			<TEXTFIELD w="15" title="Mobile No.">mobile</TEXTFIELD>
			<TEXTFIELD w="15" title="E Mail">email</TEXTFIELD>
			<TEXTFIELD w="15" title="Start Date">startdate</TEXTFIELD>
			<TEXTFIELD w="15" title="Payroll">payroll</TEXTFIELD>
			<TEXTFIELD w="15" title="Member Login">memberlogin</TEXTFIELD>
			<TEXTFIELD w="15" title="Details">details</TEXTFIELD>
			<JASPER name="Member Payment" showpdf="false" linkfield="filterid" reportfile="membermonthlyview.jasper"></JASPER>
		</GRID>
	</DESK>
	<DESK w="700" name="My Loan" key="24" h="500">
		<GRID name="My Loan" keyfield="loanid" linkfield="loanid" display="form" table="loanview" user="memberid">
			<TEXTFIELD w="15" title="Loan ID">loanid</TEXTFIELD>
			<TEXTFIELD w="15" title="Loan Type">loantypename</TEXTFIELD>
			<TEXTFIELD w="15" title="Approved">loanapproved</TEXTFIELD>
			<TEXTFIELD w="15" title="Member ID">memberid</TEXTFIELD>
			<TEXTFIELD w="15" title="Member Name">membername</TEXTFIELD>
			<TEXTFIELD w="15" title="Staff No">staffno</TEXTFIELD>
			<TEXTFIELD w="15" title="ID Number">idnumber</TEXTFIELD>
			<TEXTFIELD w="15" title="Payroll">payroll</TEXTFIELD>
			<TEXTFIELD w="15" title="Loan Date" format="date">loandate</TEXTFIELD>
			<TEXTFIELD w="15" title="Principle">principle</TEXTFIELD>
			<TEXTFIELD w="15" title="Interest Rate">interest</TEXTFIELD>
			<TEXTFIELD w="15" title="calculated Repayment Period">calcrepaymentperiod</TEXTFIELD>
			<TEXTFIELD w="15" title="Repayment Period">repaymentperiod</TEXTFIELD>
			<TEXTFIELD w="15" title="monthly Repayment">monthlyrepayment</TEXTFIELD>
			<TEXTFIELD w="15" title="Repayment Amount">repaymentamount</TEXTFIELD>
			<TEXTFIELD w="15" title="Total Repayment">totalrepayment</TEXTFIELD>
			<TEXTFIELD w="15" title="Total Interest">totalinterest</TEXTFIELD>
			<TEXTFIELD w="15" title="Loan Balance">loanbalance</TEXTFIELD>
			<JASPER name="Loan Payment Stages" showpdf="false" linkfield="filterid" reportfile="loanpaymentview.jasper"></JASPER>
			<JASPER name="Loan Payment" showpdf="false" linkfield="filterid" reportfile="loanmonthlyviewb.jasper"></JASPER>
			<FORM name="Apply For Loan" table="loans">
				<USERFIELD>memberid</USERFIELD>
				<COMBOBOX w="15" title="Loan Type" lptable="loantypes" lpfield="loantypename">loantypeid</COMBOBOX>
				<TEXTFIELD w="15" title="Principle">principle</TEXTFIELD>
				<TEXTFIELD w="15" title="Interest">interest</TEXTFIELD>
				<TEXTFIELD w="15" title="Monthly Repayment">monthlyrepayment</TEXTFIELD>
				<TEXTFIELD w="15" title="Repayment Period">repaymentperiod</TEXTFIELD>
				<TEXTAREA w="15" title="Details">details</TEXTAREA>
			</FORM>
		</GRID>
		<GRID name="Loan Types" table="loantypes">
			<TEXTFIELD w="25" title="Loan Type ID">loantypeid</TEXTFIELD>
			<TEXTFIELD w="120" title="Loan Type Name">loantypename</TEXTFIELD>
			<TEXTFIELD w="50" title="Interest Rate %">defaultinterest</TEXTFIELD>
			<TEXTFIELD w="250" title="Details">details</TEXTFIELD>
		</GRID>
	</DESK>
</APP>
