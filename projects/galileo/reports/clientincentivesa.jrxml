	<queryString>
		<![CDATA[SELECT clientid, clientname, AccountPeriod, periodyear, quarter, periodmonth, nasegs, clientcost,
clientbalance, amadeoussegs,
(CASE WHEN clientcost = 0 THEN 0 ELSE (100 * clientbalance / clientcost ) END) as pbalance,
(CASE WHEN (AmadeousSegs + NASegs) = 0 THEN 0
WHEN AmadeousSegs < 0 THEN 100
ELSE (100 * NASegs / (AmadeousSegs + NASegs)) END) as compratio,
getIncentive(rs_type, clientid, Startdate, clientcost, nasegs, amadeoussegs) as incentive,
CAST(quarter as varchar(3)) as quarterid
FROM vw_GroupCosts
WHERE (clientid = '$P!{filterid}')
AND (periodyear = '$P!{periodyearid}')
AND (quarter = '$P!{quarterid}')
AND (rs_type = 'C')
ORDER BY startdate;]]>
	</queryString>
