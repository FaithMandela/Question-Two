		<![CDATA[SELECT ClientCostView.clientid, ClientCostView.clientname, ClientCostView.periodyear, ClientCostView.periodmonth, ClientCostView.nasegs, ClientCostView.clientcost, ClientCostView.clientbalance, ClientCostView.pbalance, vw_consultanttransactions.prod, (vw_consultanttransactions.prod - ClientCostView.clientcost) as rbalance,
(CASE WHEN ClientCostView.clientcost = 0 THEN 100 ELSE (vw_consultanttransactions.prod - ClientCostView.clientcost)/ ClientCostView.clientcost END) as rbratio

FROM ClientCostView INNER JOIN vw_consultanttransactions ON 
(ClientCostView.clientid = vw_consultanttransactions.clientid) AND
(ClientCostView.periodid = vw_consultanttransactions.periodid)
