	<queryString>
		<![CDATA[SELECT transactionview.clientname, transactionview.fullname, transactionview.startdate, 
getsegs(clientid, periodid, 'G') as galileosegs,
transactionview.nasegs, transactionview.amadeoussegs,
(CASE WHEN transactionview.amadeoussegs is null THEN getsegs(clientid, periodid, 'G')
ELSE (getsegs(clientid, periodid, 'G') + transactionview.amadeoussegs) END) as totalsegs, 
transactionview.clientcost,
(CASE WHEN ((getsegs(clientid, periodid, 'G') + transactionview.amadeoussegs)) = 0 THEN 100 
ELSE (100 * getsegs(clientid, periodid, 'G') / ((getsegs(clientid, periodid, 'G') + transactionview.amadeoussegs))) END) as marketshare
FROM transactionview
WHERE (periodid = '$P!{filterid}')
ORDER BY fullname, clientname;]]>
