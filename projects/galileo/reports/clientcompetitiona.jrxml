	<queryString>
		<![CDATA[SELECT transactionview.fullname, transactionview.startdate, sum(getsegs(clientid, periodid, 'G')) as sumnasegs, 
sum(transactionview.amadeoussegs) as sumamadeous,
(CASE WHEN sum(transactionview.amadeoussegs) is null THEN sum(getsegs(clientid, periodid, 'G'))
ELSE (sum(getsegs(clientid, periodid, 'G')) + sum(transactionview.amadeoussegs)) END) as totalsegs, 
sum(transactionview.clientcost) as sumclientcost,
(CASE WHEN (sum(getsegs(clientid, periodid, 'G')) + sum(transactionview.amadeoussegs)) = 0 THEN 100 
ELSE (100 * sum(getsegs(clientid, periodid, 'G')) / (sum(getsegs(clientid, periodid, 'G')) + sum(transactionview.amadeoussegs))) END) as marketshare
FROM transactionview
WHERE (periodid = '$P!{filterid}')
GROUP BY transactionview.fullname, transactionview.startdate
ORDER BY transactionview.fullname;]]>
	</queryString>
