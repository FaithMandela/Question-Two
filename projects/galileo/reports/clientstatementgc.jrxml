	<queryString>
		<![CDATA[SELECT clientid, clientname, AccountPeriod, periodyear, quarter, periodmonth, nasegs, clientcost,
clientbalance, pbalance, amadeoussegs, (100 - compratio) as marketshare,
getIncentive(rs_type, clientid, Startdate, clientcost, nasegs, amadeoussegs) as incentive,
CAST(quarter as varchar(3)) as quarterid
FROM vw_GroupCosts
WHERE (clientid = '$P!{filterid}')
