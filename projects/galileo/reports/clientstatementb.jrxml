		<![CDATA[SELECT clientid, clientname, address, town, zipcode, country, quarter,
periodyear, periodmonth, nasegs, clientcost, clientbalance, pbalance,
(AmadeousSegs + NASegs) as tsegs,
(CASE WHEN (AmadeousSegs + NASegs) = 0 THEN 0 ELSE 
(100 * NASegs / (AmadeousSegs + NASegs)) END) as marketratio,
(clientcost - nasegs) as charge_variance
FROM ClientCostView
WHERE (clientid = '$P!{filterid}')
AND (periodyear='$P!{yearid}')
