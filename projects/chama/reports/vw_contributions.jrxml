		<queryString>
			<![CDATA[SELECT entity_id, entity_name, org_id, SUM(investment_amount) AS inv, SUM(merry_go_round_amount) as mgr, SUM(loan_contrib) as loan, SUM(total_contribution) as total
 FROM vw_contributions
