vw_statement.invoice_amount, vw_statement.credit_amount, payments, countryname, address, postalcode, town
FROM vw_statement
JOIN vwclients ON vw_statement.clientid = vwclients.clientid

WHERE vw_statement.clientid = '$P!{clientid}']]>
