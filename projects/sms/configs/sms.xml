<APP name="Baraza SMS" org="org_id" password="change_password">
	<MENU name="Baraza SMS">
		<MENU name="SMS">1</MENU>
		<MENU name="Address">5</MENU>
		<MENU name="Organisation" role="admin">10</MENU>
		<MENU name="Entitys" role="admin">15</MENU>
		<MENU name="Reports">
			<MENU name="SMS">105</MENU>
			<MENU name="SMS Summary">110</MENU>
		</MENU>
	</MENU>
	<DESK w="700" name="SMS" key="1" h="500">
		<GRID name="Outbox" keyfield="sms_id" orderby="sms_id desc" table="vw_sms" where="(folder_id = 0) AND (address_group_id is null)">
			<TEXTFIELD w="75" title="SMS ID">sms_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Tel. Number">sms_number</TEXTFIELD>
			<CHECKBOX w="75" title="Message Ready" format="boolean">message_ready</CHECKBOX>
			<CHECKBOX w="75" title="Sent" format="boolean">sent</CHECKBOX>
			<TEXTAREA w="175" title="message">message</TEXTAREA>
			<FORM name="SMS" keyfield="sms_id" table="sms">
				<DEFAULT default="0">folder_id</DEFAULT>
				<USERFIELD>entity_id</USERFIELD>
				<TEXTAREA w="430" title="Numbers" y="120" h="40" x="10">sms_numbers</TEXTAREA>
				<CHECKBOX w="150" title="Message Ready" default="true" y="30" h="20" x="10" rows="4">message_ready</CHECKBOX>
				<TEXTAREA w="430" tab="Message" y="50" h="70" x="10">message</TEXTAREA>
				<TEXTAREA w="430" tab="Details" y="120" h="70" x="10">details</TEXTAREA>
			</FORM>
			<GRID name="SMS Address" keyfield="sms_address_id" linkfield="sms_id" table="vw_sms_address">
				<TEXTFIELD w="75" title="Name">address_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Mobile Number">mobile</TEXTFIELD>
				<TEXTFIELD w="75" title="Narrative">narrative</TEXTFIELD>
				<FORM name="SMS Address" keyfield="sms_address_id" linkfield="sms_id" table="sms_address">
					<GRIDBOX w="430" title="Address" lptable="address" y="10" h="20" x="10" lpfield="address_id">address_id
						<GRID name="Address" keyfield="address_id" table="vw_address" where="table_name = 'sms'">
							<TEXTFIELD w="75" title="Name">address_name</TEXTFIELD>
							<TEXTFIELD w="75" title="Mobile">mobile</TEXTFIELD>
						</GRID>
					</GRIDBOX>
					<TEXTFIELD w="430" title="Narrative" y="30" h="20" x="10">narrative</TEXTFIELD>
				</FORM>
			</GRID>
			<GRID name="Add Address" keyfield="address_id" table="vw_address" where="table_name = 'sms'">
				<TEXTFIELD w="175" title="Name">address_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Mobile">mobile</TEXTFIELD>
				<ACTIONS>
					<ACTION fnct="ins_sms_address">Add</ACTION>
				</ACTIONS>
			</GRID>
		</GRID>
		<GRID name="Group SMS" keyfield="sms_id" orderby="sms_id desc" table="vw_sms" where="(folder_id = 0) AND (address_group_id is not null)">
			<TEXTFIELD w="75" title="SMS ID">sms_id</TEXTFIELD>
			<CHECKBOX w="75" title="Message Ready" format="boolean">message_ready</CHECKBOX>
			<CHECKBOX w="75" title="Sent" format="boolean">sent</CHECKBOX>
			<TEXTFIELD w="75" title="Address Group Name">address_group_name</TEXTFIELD>
			<TEXTAREA w="175" title="message">message</TEXTAREA>
			<FORM name="SMS" keyfield="sms_id" table="sms">
				<DEFAULT default="0">folder_id</DEFAULT>
				<USERFIELD>entity_id</USERFIELD>
				<COMBOBOX w="430" title="Group" lptable="address_groups" y="30" h="20" x="10" lpfield="address_group_name">address_group_id</COMBOBOX>
				<CHECKBOX w="150" title="Message Ready" y="30" h="20" x="10">message_ready</CHECKBOX>
				<TEXTAREA w="430" tab="Message" y="50" h="70" x="10">message</TEXTAREA>
				<TEXTAREA w="430" tab="Details" y="120" h="70" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
		<GRID name="Sent" keyfield="sms_id" orderby="sms_id desc" table="vw_sms" where="folder_id = 2">
			<TEXTFIELD w="75" title="SMS Id">sms_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Tel. Number">sms_number</TEXTFIELD>
			<TEXTFIELD w="75" title="Name">address_name</TEXTFIELD>
			<CHECKBOX w="75" title="Sent">sent</CHECKBOX>
			<TEXTAREA w="175" title="message">message</TEXTAREA>
			<GRID name="Sms Queue" keyfield="sms_queue_id" linkfield="sms_id" table="sms_queue">
				<CHECKBOX w="75" title="Message Sent">message_sent</CHECKBOX>
				<TEXTFIELD w="75" title="Status Code">status_code</TEXTFIELD>
				<TEXTFIELD w="75" title="Delivery Status">delivery_status</TEXTFIELD>
				<TEXTFIELD w="75" title="Message Parts">message_parts</TEXTFIELD>
				<TEXTFIELD w="75" title="Number">sms_number</TEXTFIELD>
				<TEXTDECIMAL w="75" title="Price">sms_price</TEXTDECIMAL>
			</GRID>
			<GRID name="SMS Address" keyfield="sms_address_id" linkfield="sms_id" table="vw_sms_address">
				<TEXTFIELD w="75" title="Entity Name">address_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Mobile Number">mobile</TEXTFIELD>
				<TEXTFIELD w="75" title="Narrative">narrative</TEXTFIELD>
			</GRID>
			<GRID name="SMS Groups" keyfield="sms_groups_id" linkfield="sms_id" table="vw_sms_groups">
				<TEXTFIELD w="75" title="Entity Type">entity_type_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Narrative">narrative</TEXTFIELD>
			</GRID>
		</GRID>
		<GRID new="false" name="Inbox" keyfield="sms_id" orderby="sms_id desc" table="vw_sms" where="folder_id = 3">
			<TEXTFIELD w="75" title="SMS Id">sms_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Tel. Number">sms_number</TEXTFIELD>
			<TEXTFIELD w="75" title="Name">address_name</TEXTFIELD>
			<TEXTAREA w="275" title="message">message</TEXTAREA>
			<FORM new="false" name="SMS" keyfield="sms_id" table="sms">
				<TEXTAREA w="430" title="Message" y="10" h="110" disabled="true" x="10">message</TEXTAREA>
				<TEXTAREA w="430" title="Details" y="120" h="70" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
		<GRID new="false" name="Action" keyfield="sms_id" orderby="sms_id desc" table="vw_sms" where="folder_id = 4">
			<TEXTFIELD w="75" title="SMS Id">sms_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Tel. Number">sms_number</TEXTFIELD>
			<TEXTFIELD w="75" title="Name">address_name</TEXTFIELD>
			<TEXTAREA w="275" title="message">message</TEXTAREA>
			<FORM new="false" name="SMS" keyfield="sms_id" table="sms">
				<TEXTAREA w="430" title="Message" y="10" h="110" disabled="true" x="10">message</TEXTAREA>
				<TEXTAREA w="430" title="Details" y="120" h="70" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
	</DESK>
	<DESK w="700" name="Address" key="5" h="550">
		<GRID name="Address" keyfield="address_id" table="vw_address" where="(table_name = 'sms')">
			<TEXTFIELD w="75" title="Country Name">sys_country_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Name">address_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Mobile">mobile</TEXTFIELD>
			<TEXTFIELD w="75" title="Email">email</TEXTFIELD>
			<GRID name="Group" keyfield="address_member_id" linkfield="address_id" table="vw_address_members">
				<TEXTFIELD w="75" title="Group">address_group_name</TEXTFIELD>
				<CHECKBOX w="75" title="Active">is_active</CHECKBOX>
				<TEXTFIELD w="75" title="Narrative">narrative</TEXTFIELD>
				<FORM name="Group" keyfield="address_member_id" linkfield="address_id" table="address_members">
					<COMBOBOX w="430" title="Group" lptable="address_groups" y="10" h="20" x="10" lpfield="address_group_name">address_group_id</COMBOBOX>
					<CHECKBOX w="150" title="Active" y="30" h="20" x="10">is_active</CHECKBOX>
					<TEXTFIELD w="430" title="Narrative" y="50" h="20" x="10">narrative</TEXTFIELD>
				</FORM>
			</GRID>
			<FORM name="Address" keyfield="address_id" table="address">
				<DEFAULT title="Table Name" default="sms">table_name</DEFAULT>
				<COMBOBOX w="430" title="Address Type" lptable="address_types" y="10" h="20" x="10" lpfield="address_type_name">address_type_id</COMBOBOX>
				<COMBOBOX w="430" title="Country" default="KE" lptable="sys_countrys" noorg="true" y="30" h="20" x="10" lpfield="sys_country_name">sys_country_id</COMBOBOX>
				<TEXTFIELD w="430" title="Name" y="50" h="20" x="10">address_name</TEXTFIELD>
				<TEXTFIELD w="430" title="Mobile" y="70" h="20" x="10">mobile</TEXTFIELD>
				<TEXTFIELD w="150" title="Phone Number" y="90" h="20" x="10">phone_number</TEXTFIELD>
				<TEXTFIELD w="150" title="Extension" y="90" h="20" x="290">extension</TEXTFIELD>
				<TEXTFIELD w="150" title="Post Office Box" y="110" h="20" x="10">post_office_box</TEXTFIELD>
				<TEXTFIELD w="150" title="Postal Code" y="110" h="20" x="290">postal_code</TEXTFIELD>
				<TEXTFIELD w="430" title="Premises" y="130" h="20" x="10">premises</TEXTFIELD>
				<TEXTFIELD w="430" title="Street" y="150" h="20" x="10">street</TEXTFIELD>
				<TEXTFIELD w="150" title="Town" y="170" h="20" x="10">town</TEXTFIELD>
				<TEXTFIELD w="430" title="Fax" y="190" h="20" x="10">fax</TEXTFIELD>
				<TEXTFIELD w="430" title="Email" y="210" h="20" x="10">email</TEXTFIELD>
				<TEXTFIELD w="430" title="Website" y="230" h="20" x="10">website</TEXTFIELD>
				<TEXTAREA w="430" title="Details" y="250" h="70" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
		<GRID name="Address Groups" keyfield="address_group_id" table="address_groups">
			<TEXTFIELD w="75" title="Group ID">address_group_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Group Name">address_group_name</TEXTFIELD>
			<GRID name="Members" keyfield="address_member_id" linkfield="address_group_id" table="vw_address_members">
				<TEXTFIELD w="75" title="Name">address_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Mobile">mobile</TEXTFIELD>
				<CHECKBOX w="75" title="Active">is_active</CHECKBOX>
				<TEXTFIELD w="75" title="Narrative">narrative</TEXTFIELD>
				<FORM name="Members" keyfield="address_member_id" linkfield="address_group_id" table="address_members">
					<COMBOBOX w="430" title="Name" lptable="address" y="10" h="20" x="10" lpfield="address_name">address_id</COMBOBOX>
					<CHECKBOX w="150" title="Active" default="true" y="30" h="20" x="10">is_active</CHECKBOX>
					<TEXTFIELD w="430" title="Narrative" y="50" h="20" x="10">narrative</TEXTFIELD>
				</FORM>
			</GRID>
			<GRID name="Add Members" keyfield="address_id" table="vw_address" where="table_name = 'sms'">
				<TEXTFIELD w="175" title="Name">address_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Mobile">mobile</TEXTFIELD>
				<ACTIONS>
					<ACTION fnct="ins_member_address">Add</ACTION>
				</ACTIONS>
			</GRID>
			<FORM name="Address Groups" keyfield="address_group_id" table="address_groups">
				<TEXTFIELD w="150" title="Address Group Name" y="10" h="20" x="10">address_group_name</TEXTFIELD>
				<TEXTAREA w="430" title="Details" y="30" h="70" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
		<GRID name="Address Types" keyfield="address_type_id" table="address_types">
			<TEXTFIELD w="75" title="Address Type ID">address_type_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Address Type Name">address_type_name</TEXTFIELD>
			<FORM name="Address Types" keyfield="address_type_id" table="address_types">
				<TEXTFIELD w="150" title="Address Type Name" y="10" h="20" x="10">address_type_name</TEXTFIELD>
			</FORM>
		</GRID>
	</DESK>
	<DESK w="700" name="Organisation" key="10" h="500">
		<GRID name="Organisation" keyfield="org_id" table="orgs">
			<TEXTFIELD w="75" title="Org ID">org_id</TEXTFIELD>
			<TEXTFIELD w="275" title="Org Name">org_name</TEXTFIELD>
			<CHECKBOX w="75" title="Default">is_default</CHECKBOX>
			<CHECKBOX w="75" title="Active">is_active</CHECKBOX>
			<GRID name="Address" keyfield="address_id" linkfield="table_id" table="vw_address" where="table_name = 'orgs'">
				<TEXTFIELD w="140" title="Country">sys_country_name</TEXTFIELD>
				<TEXTFIELD w="80" title="Postal Code">postal_code</TEXTFIELD>
				<TEXTFIELD w="140" title="Town">town</TEXTFIELD>
				<TEXTFIELD w="140" title="Phone Number">phone_number</TEXTFIELD>
				<TEXTFIELD w="120" title="Mobile">mobile</TEXTFIELD>
				<TEXTFIELD w="120" title="Email">email</TEXTFIELD>
				<FORM name="Address" keyfield="address_id" linkfield="table_id" table="address">
					<DEFAULT default="orgs">table_name</DEFAULT>
					<COMBOBOX w="430" title="Country" default="KE" lptable="sys_countrys" noorg="true" y="10" h="20" x="10" lpfield="sys_country_name">sys_country_id</COMBOBOX>
					<TEXTFIELD w="150" title="Post Office Box" y="30" h="20" x="10">post_office_box</TEXTFIELD>
					<TEXTFIELD w="150" title="Postal Code" y="30" h="20" x="290">postal_code</TEXTFIELD>
					<TEXTFIELD w="150" title="Town" y="50" h="20" x="10">town</TEXTFIELD>
					<TEXTFIELD w="430" title="Premises" y="70" h="20" x="10">premises</TEXTFIELD>
					<TEXTFIELD w="430" title="Street" y="90" h="20" x="10">street</TEXTFIELD>
					<TEXTFIELD w="430" title="Phone Number" y="110" h="20" x="10">phone_number</TEXTFIELD>
					<TEXTFIELD w="150" title="Extension" y="130" h="20" x="10">extension</TEXTFIELD>
					<TEXTFIELD w="430" title="Mobile" y="150" h="20" x="10">mobile</TEXTFIELD>
					<TEXTFIELD w="430" title="Fax" y="170" h="20" x="10">fax</TEXTFIELD>
					<TEXTFIELD w="430" title="Email" y="190" h="20" x="10">email</TEXTFIELD>
					<TEXTAREA w="430" title="Details" y="210" h="70" x="10">details</TEXTAREA>
				</FORM>
			</GRID>
			<FORM name="Organisation" keyfield="org_id" table="orgs" noorg="true">
				<TEXTFIELD w="350" title="Organisation" y="10" h="20" x="10">org_name</TEXTFIELD>
				<CHECKBOX w="150" title="Default" y="30" h="20" x="10">is_default</CHECKBOX>
				<CHECKBOX w="150" title="Active" y="30" h="20" x="290">is_active</CHECKBOX>
				<TEXTFIELD w="350" title="Logo" y="50" h="20" x="10">logo</TEXTFIELD>
				<COMBOBOX w="430" title="Base Currency" lptable="currency" y="70" h="20" x="10" lpfield="currency_name">currency_id</COMBOBOX>
				<COMBOBOX w="430" title="Parent Org" lptable="orgs" lpkey="org_id" y="90" h="20" x="10" lpfield="org_name">parent_org_id</COMBOBOX>
				<TEXTFIELD w="50" title="Sufix" y="110" h="20" x="10">org_sufix</TEXTFIELD>
				<TEXTFIELD w="150" title="PCC" y="130" h="20" x="10">pcc</TEXTFIELD>
				<TEXTFIELD w="150" title="SP ID" y="130" h="20" x="290">sp_id</TEXTFIELD>
				<TEXTFIELD w="150" title="Service ID" y="150" h="20" x="10">service_id</TEXTFIELD>
				<TEXTFIELD w="150" title="Sender Name" y="150" h="20" x="290">sender_name</TEXTFIELD>
				<TEXTDECIMAL w="150" title="SMS Rate" default="2" y="170" h="20" x="10">sms_rate</TEXTDECIMAL>
				<TEXTAREA w="430" title="Details" y="190" h="70" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
		<GRID name="Currency" keyfield="currency_id" table="currency" noorg="true">
			<TEXTFIELD w="75" title="Currency ID">currency_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Currency Name">currency_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Currency Symbol">currency_symbol</TEXTFIELD>
			<GRID name="Currency Rates" keyfield="currency_rate_id" linkfield="currency_id" table="currency_rates">
				<TEXTFIELD w="75" title="Currency Rate ID">currency_rate_id</TEXTFIELD>
				<TEXTDATE w="75" title="Exchange Date">exchange_date</TEXTDATE>
				<TEXTDECIMAL w="75" title="Exchange Rate">exchange_rate</TEXTDECIMAL>
				<FORM name="Currency Rates" keyfield="currency_rate_id" linkfield="currency_id" table="currency_rates">
					<TEXTDATE w="150" title="Exchange Date" y="10" h="20" x="10">exchange_date</TEXTDATE>
					<TEXTDECIMAL w="150" title="Exchange Rate" y="10" h="20" x="290">exchange_rate</TEXTDECIMAL>
				</FORM>
			</GRID>
		</GRID>
		<GRID name="Receipts" keyfield="receipt_id" linkfield="org_id" table="receipts">
			<TEXTFIELD w="75" title="Receipt ID">receipt_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Mpesa Trx ID">mpesa_trx_id</TEXTFIELD>
			<TEXTDATE w="75" title="Receipt Date">receipt_date</TEXTDATE>
			<TEXTDECIMAL w="75" title="Receipt Amount">receipt_amount</TEXTDECIMAL>
			<FORM name="Receipts" keyfield="receipt_id" linkfield="org_id" table="receipts">
				<TEXTDATE w="150" title="Receipt Date" y="10" h="20" x="10">receipt_date</TEXTDATE>
				<TEXTDECIMAL w="150" title="Receipt Amount" y="10" h="20" x="290">receipt_amount</TEXTDECIMAL>
				<TEXTAREA w="430" title="Details" y="30" h="70" x="10">details</TEXTAREA>
			</FORM>
		</GRID>
	</DESK>
	<DESK w="700" name="Entitys" key="15" h="500">
		<GRID name="Entitys" keyfield="entity_id" table="vw_entitys">
			<TEXTFIELD w="75" title="Entity ID">entity_id</TEXTFIELD>
			<TEXTFIELD w="140" title="Entity Name">entity_name</TEXTFIELD>
			<TEXTFIELD w="100" title="User Name">user_name</TEXTFIELD>
			<TEXTFIELD w="100" title="Role">function_role</TEXTFIELD>
			<CHECKBOX w="100" title="Super User" format="boolean">super_user</CHECKBOX>
			<CHECKBOX w="75" title="Active" format="boolean">is_active</CHECKBOX>
			<TEXTFIELD w="100" title="Type">entity_type_name</TEXTFIELD>
			<TEXTFIELD w="75" title="Email">primary_email</TEXTFIELD>
			<TEXTFIELD w="75" title="Telephone">primary_telephone</TEXTFIELD>
			<GRID name="Address" keyfield="address_id" linkfield="table_id" table="vw_address" where="table_name = 'entitys'">
				<TEXTFIELD w="100" title="Country">sys_country_name</TEXTFIELD>
				<TEXTFIELD w="100" title="Postal Code">postal_code</TEXTFIELD>
				<TEXTFIELD w="100" title="Town">town</TEXTFIELD>
				<TEXTFIELD w="100" title="Phone Number">phone_number</TEXTFIELD>
				<TEXTFIELD w="100" title="Mobile">mobile</TEXTFIELD>
				<TEXTFIELD w="140" title="Email">email</TEXTFIELD>
				<TEXTFIELD w="140" title="Default">is_default</TEXTFIELD>
				<FORM name="Address" keyfield="address_id" linkfield="table_id" table="address">
					<DEFAULT default="entitys">table_name</DEFAULT>
					<COMBOBOX w="430" title="Country" default="KE" lptable="sys_countrys" noorg="true" y="10" h="20" x="10" lpfield="sys_country_name">sys_country_id</COMBOBOX>
					<TEXTFIELD w="150" title="Post Office Box" y="30" h="20" x="10">post_office_box</TEXTFIELD>
					<TEXTFIELD w="150" title="Postal Code" y="30" h="20" x="290">postal_code</TEXTFIELD>
					<TEXTFIELD w="150" title="Town" y="50" h="20" x="10">town</TEXTFIELD>
					<TEXTFIELD w="430" title="Premises" y="70" h="20" x="10">premises</TEXTFIELD>
					<TEXTFIELD w="430" title="Street" y="90" h="20" x="10">street</TEXTFIELD>
					<TEXTFIELD w="430" title="Phone Number" y="110" h="20" x="10">phone_number</TEXTFIELD>
					<TEXTFIELD w="150" title="Extension" y="130" h="20" x="10">extension</TEXTFIELD>
					<TEXTFIELD w="430" title="Mobile" y="150" h="20" x="10">mobile</TEXTFIELD>
					<TEXTFIELD w="430" title="Fax" y="170" h="20" x="10">fax</TEXTFIELD>
					<TEXTFIELD w="430" title="Email" y="190" h="20" x="10">email</TEXTFIELD>
					<CHECKBOX w="30" title="Default" y="210" h="20" x="10">is_default</CHECKBOX>
					<TEXTAREA w="430" title="Details" y="230" h="70" x="10">details</TEXTAREA>
				</FORM>
			</GRID>
			<GRID name="Subscriptions" keyfield="entity_subscription_id" linkfield="entity_id" table="vw_entity_subscriptions">
				<TEXTFIELD w="75" title="Entity Type Name">entity_type_name</TEXTFIELD>
				<TEXTFIELD w="75" title="Subscription Level Name">subscription_level_name</TEXTFIELD>
				<FORM name="Subscriptions" keyfield="entity_subscription_id" linkfield="entity_id" table="entity_subscriptions">
					<COMBOBOX w="430" title="Entity Type" lptable="entity_types" y="10" h="20" x="10" lpfield="entity_type_name">entity_type_id</COMBOBOX>
					<COMBOBOX w="430" title="Subscription Level" lptable="subscription_levels" y="30" h="20" x="10" lpfield="subscription_level_name">subscription_level_id</COMBOBOX>
					<TEXTAREA w="430" title="Details" y="50" h="70" x="10">details</TEXTAREA>
				</FORM>
			</GRID>
			<FORM name="Entitys" keyfield="entity_id" table="entitys">
				<COMBOBOX w="430" title="Org" lptable="orgs" y="10" h="20" x="10" lpfield="org_name">org_id</COMBOBOX>
				<COMBOBOX w="430" title="Entity Type" lptable="entity_types" y="30" h="20" x="10" lpfield="entity_type_name">entity_type_id</COMBOBOX>
				<TEXTFIELD w="150" title="Entity Name" y="50" h="20" x="10">entity_name</TEXTFIELD>
				<TEXTFIELD w="150" title="User Name" y="50" h="20" x="290">user_name</TEXTFIELD>
				<TEXTFIELD w="430" title="Primary Email" y="70" h="20" x="10">primary_email</TEXTFIELD>
				<TEXTFIELD w="250" title="Primary Telephone" y="90" h="20" x="10">primary_telephone</TEXTFIELD>
				<CHECKBOX w="150" title="Super User" y="110" h="20" x="10">super_user</CHECKBOX>
				<CHECKBOX w="150" title="Entity Leader" y="110" h="20" x="290">entity_leader</CHECKBOX>
				<CHECKBOX w="150" title="No Org" y="130" h="20" x="10">no_org</CHECKBOX>
				<TEXTFIELD w="150" title="Function Role" y="130" h="20" x="290">function_role</TEXTFIELD>
