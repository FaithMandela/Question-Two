				<TEXTFIELD w="75" title="Rate Type">rate_plan_name</TEXTFIELD>
				<TEXTFIELD w="75" title="individual">individual</TEXTFIELD>
				<TEXTFIELD w="75" title="Others">others</TEXTFIELD>
				<FORM noorg="true" keyfield="benefit_id" name="Benefits" linkfield="rate_type_id" table="benefits">
					<COMBOBOX w="430" h="20" lpfield="benefit_type_name" x="10" y="10" lptable="benefit_types" title="Benefit Type ID">benefit_type_id</COMBOBOX>
					<TEXTAREA w="430" h="70" x="10" y="30" title="Individual">individual</TEXTAREA>
					<TEXTAREA w="430" h="70" x="10" y="100" title="Others">others</TEXTAREA>
