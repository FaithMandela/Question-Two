	<DESK w="700" h="550" name="Subscription Levels" key="15">
		<GRID keyfield="subscription_level_id" name="Subscription Levels" table="subscription_levels">
			<TEXTFIELD w="75" title="Subscription Level ID">subscription_level_id</TEXTFIELD>
