					<TEXTFIELD w="150" h="20" x="10" y="90" title="Primary Telephone">primary_telephone</TEXTFIELD>
					<TEXTFIELD w="430" h="20" x="10" y="130" title="Function Role">function_role</TEXTFIELD>
					<CHECKBOX w="150" h="20" x="290" y="150" title="Active">is_active</CHECKBOX>
					<TEXTFIELD w="150" h="20" x="290" y="170" title="First Password">first_password</TEXTFIELD>
					<TEXTAREA w="430" h="70" x="10" y="230" title="Details">details</TEXTAREA>
				</FORM>
			</GRID>
			<FORM keyfield="org_id" name="Orgs" table="orgs">
				<COMBOBOX w="430" h="20" lpfield="currency_name" x="10" y="10" lptable="currency" title="Currency ID">currency_id</COMBOBOX>
				<TEXTFIELD w="150" h="20" x="10" y="50" title="Org Name">org_name</TEXTFIELD>
				<TEXTFIELD w="150" h="20" x="290" y="50" title="Org Sufix">org_sufix</TEXTFIELD>
				<TEXTFIELD w="150" h="20" x="10" y="70" title="PCC">pcc</TEXTFIELD>
				<CHECKBOX w="150" h="20" x="290" y="70" title="Active">is_active</CHECKBOX>
