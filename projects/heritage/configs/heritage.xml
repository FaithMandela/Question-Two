			</FORM>
		</GRID>
	</DESK>
	<DESK w="700" h="550" name="Entity Types" key="25">
		<GRID keyfield="entity_type_id" name="Entity Types" table="entity_types">
			<TEXTFIELD w="75" title="Entity Type ID">entity_type_id</TEXTFIELD>
			<TEXTFIELD w="75" title="Org ID">org_id</TEXTFIELD>
