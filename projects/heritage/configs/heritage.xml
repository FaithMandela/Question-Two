					<TEXTDECIMAL w="150" h="20" x="10" y="30" title="Excluding USA and Canada">standard_rate</TEXTDECIMAL>
					<TEXTDECIMAL w="150" h="20" x="290" y="30" title="Including USA and Canada">north_america_rate</TEXTDECIMAL>
				</FORM>
			</GRID>
			<GRID noorg="true" keyfield="benefit_id" name="Benefits" tab.count="count(benefit_id)" linkfield="rate_type_id" table="vw_benefits">
