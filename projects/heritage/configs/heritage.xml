			<TILE w="60" name="Today Sales" role="user,agency,broker" noorg.query="true" table="passengers" where="(DATE(approved_date)=CURRENT_DATE)" h="25">
				<TEXTFIELD w="75" fnct="COALESCE(count(passenger_id),0)" title="Total Sales">total_passengers</TEXTFIELD>
			</TILE>
			<TILE name="Total Amount" role="user,agency" noorg.query="true" table="vw_passengers" where="approved is true">
				<TEXTFIELD w="75" fnct="ROUND(sum(totalamount_covered)::numeric,2)" title="Total Amount">Total_covered</TEXTFIELD>
			</TILE>
			<TILE w="60" name="Today Sales" role="admin,manager" noorg.query="true" table="vw_passengers" noorg="true" where="(DATE(approved_date)=CURRENT_DATE) AND approved is true" h="25">
				<TEXTFIELD w="75" fnct="COALESCE(count(passenger_id), 0)" title="Total Members">total_passengers</TEXTFIELD>
			</TILE>
			<TILE name="Total Amount" role="admin,manager" noorg.query="true" table="vw_passengers" noorg="true" where="approved is true">
				<TEXTFIELD w="75" fnct="ROUND(sum(totalamount_covered)::numeric,2)" title="Total Amount">Total_covered</TEXTFIELD>
			</TILE>
