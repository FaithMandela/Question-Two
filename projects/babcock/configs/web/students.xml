			<TEXTFIELD w="200" title="Selection">selection</TEXTFIELD>
			<TEXTFIELD w="350" title="Details">details</TEXTFIELD>
			<ACTIONS jumpview="33:0">
				<ACTION fnct="updQStudent">Select</ACTION>
			</ACTIONS>
		</GRID>
	</DESK>
	<DESK w="700" name="Outstanding Courses" key="103" h="500">
		<GRID name="Outstanding Courses" keyfield="courseid" orderby="yeartaken, courseid" table="coursechecklist" where="(coursepased = false)" user="studentid">
			<TEXTFIELD w="50" title="Year Taken">yeartaken</TEXTFIELD>
			<TEXTFIELD w="50" title="Course ID">courseid</TEXTFIELD>
			<TEXTFIELD w="240" title="Course Title">coursetitle</TEXTFIELD>
			<TEXTFIELD w="70" title="Hours">credithours</TEXTFIELD>
			<CHECKBOX w="50" title="Prereq. Passed" format="boolean" ischar="true">prereqpassed</CHECKBOX>
			<CHECKBOX w="50" title="Elective" format="boolean" ischar="true">elective</CHECKBOX>
			<GRID w="600" tw="700" name="Courses Details" lw="100" keyfield="courseid" linkfield="courseid" display="form" table="courses" noorg="true">
				<TEXTFIELD w="40" title="Course ID">courseid</TEXTFIELD>
				<TEXTFIELD w="70" title="Course Title">coursetitle</TEXTFIELD>
				<TEXTFIELD w="70" title="Hours">credithours</TEXTFIELD>
				<TEXTFIELD w="70" title="Year Taken">yeartaken</TEXTFIELD>
				<TEXTFIELD w="70" title="Details">details</TEXTFIELD>
				<GRID name="Prerequisite" linkfield="courseid" table="prereqview" noorg="true" where="(iscurrent=true)">
					<TEXTFIELD w="75" title="Course ID">courseid</TEXTFIELD>
					<TEXTFIELD w="300" title="Course Title">coursetitle</TEXTFIELD>
					<TEXTFIELD w="75" title="Level">optionlevel</TEXTFIELD>
					<TEXTFIELD w="75" title="Grade">gradeid</TEXTFIELD>
				</GRID>
			</GRID>
		</GRID>
	</DESK>
	<DESK w="700" name="Review Details" key="107" h="500">
		<FORM w="530" name="Review Details" lw="120" canedit="true" table="students" user="studentid">
			<TEXTFIELD w="40" title="Mobile No.">mobile</TEXTFIELD>
			<TEXTFIELD w="40" title="Tel No.">telno</TEXTFIELD>
			<TEXTFIELD w="40" title="E Mail">email</TEXTFIELD>
			<TEXTFIELD w="40" title="Address">address</TEXTFIELD>
			<TEXTFIELD w="40" title="Zip Code">zipcode</TEXTFIELD>
			<TEXTFIELD w="40" title="Town">town</TEXTFIELD>
			<COMBOBOX w="40" title="Religious Affiliation" lptable="denominations" lpkey="denominationid" lpfield="denominationname">denominationid</COMBOBOX>
			<COMBOBOX w="40" title="Origin State/L.G.A" lptable="states" lpkey="stateid" lpfield="statename">stateid</COMBOBOX>
		</FORM>
	</DESK>
	<DESK w="700" name="Financial Clearance" key="40" h="500">
		<GRID name="Financial Clearance" keyfield="qstudentid" table="qcurrstudentdegreeview" where="(approved = true) and (finaceapproval = true)" user="studentid">
			<TEXTFIELD w="40" title="Semester">quarterid</TEXTFIELD>
			<CHECKBOX w="40" title="Submited Registration" format="boolean" ischar="true">finalised</CHECKBOX>
			<CHECKBOX w="40" title="Finance" format="boolean" ischar="true">finaceapproval</CHECKBOX>
			<CHECKBOX w="40" title="Overload" format="boolean" ischar="true">overloadapproval</CHECKBOX>
			<CHECKBOX w="40" title="Major Advisor" format="boolean" ischar="true">majorapproval</CHECKBOX>
			<CHECKBOX w="40" title="Student Affairs" format="boolean" ischar="true">studentdeanapproval</CHECKBOX>
			<CHECKBOX w="40" title="Registar" format="boolean" ischar="true">approved</CHECKBOX>
			<CHECKBOX w="40" title="Registration Printed" format="boolean" ischar="true">printed</CHECKBOX>
			<JASPER filtered="true" name="Exam PASS" printcopies="1" reportfile="studentpass.jasper"></JASPER>
		</GRID>
	</DESK>
</APP>
