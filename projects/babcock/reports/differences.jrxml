	<queryString>
		<![CDATA[SELECT vwsuncharges.quarterid, vwsuncharges.studylevel,
	vwsuncharges.majorid, vwsuncharges.majorname, vwsuncharges.mealtype,
	vwsuncharges.accountnumber, vwsuncharges.studentname,
	(vwsuncharges.fees - vwsuncharges.residencecharge) as net_fees,
	sum(vwsuncharges.unitfees)
FROM vwsuncharges
WHERE (vwsuncharges.quarterid = '$P!{filterid}')
GROUP BY vwsuncharges.quarterid, vwsuncharges.studylevel,
	vwsuncharges.majorid, vwsuncharges.majorname, vwsuncharges.mealtype,
	vwsuncharges.accountnumber, vwsuncharges.studentname,
	vwsuncharges.fees, vwsuncharges.residencecharge
HAVING abs((vwsuncharges.fees - vwsuncharges.residencecharge) - sum(vwsuncharges.unitfees)) > 2
ORDER BY vwsuncharges.studylevel, vwsuncharges.majorid, vwsuncharges.mealtype;]]>
