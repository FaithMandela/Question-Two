	<queryString>
		<![CDATA[SELECT vwqstudentcharges.departmentname, vwqstudentcharges.quarterid,
 vwqstudentcharges.studylevel, vwqstudentcharges.majorname,
count(vwqstudentcharges.qstudentid) as sum_students,
sum(vwqstudentcharges.fees) as sum_fees

FROM vwqstudentcharges

WHERE (vwqstudentcharges.quarterid = '$P!{filterid}')
AND (vwqstudentcharges.finaceapproval = true)

GROUP BY vwqstudentcharges.departmentname, vwqstudentcharges.quarterid,
 vwqstudentcharges.studylevel, vwqstudentcharges.majorname

ORDER BY vwqstudentcharges.departmentname, vwqstudentcharges.majorname, vwqstudentcharges.studylevel;]]>
