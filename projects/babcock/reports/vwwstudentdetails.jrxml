		<![CDATA[SELECT students.studentid, (upper(students.surname) || '  ' || COALESCE(initcap(students.firstname), '') ||' '|| COALESCE(initcap(students.othernames), '')) as studentname,studentdegreeview.birthdate,studentdegreeview.schoolname,students.stateid,
studentdegreeview.addresscountry, studentdegreeview.town, studentdegreeview.sex,studentdegreeview.address,
studentdegreeview.town,studentdegreeview.nationalitycountry,studentdegreeview.schoolname,
degreeid, degreename,getcoremajor(studentdegreeid) as coremajor,studentdegreeview.religionname,
studentdegreeview.zipcode,studentdegreeview.denominationname,studentdegreeview.maritalstatus,
studentdegreeview.started,studentdegreeview.sublevelname,
studentdegreeview.guardianname, studentdegreeview.gaddress,studentdegreeview.gzipcode,
studentdegreeview.gtown, studentdegreeview.gaddress, studentdegreeview.gtelno, 
studentdegreeview.gemail

FROM (studentdegreeview INNER JOIN students ON students.studentid = studentdegreeview.studentid)
WHERE (studentdegreeid=$P!{filterid});]]>
