	<queryString>
		<![CDATA[SELECT vw_loan_monthly.entity_id, vw_loan_monthly.entity_name, vw_loan_monthly.loan_type_name, vw_loan_monthly.loan_id,
vw_loan_monthly.loan_date, vw_loan_monthly.principle, vw_loan_monthly.interest, vw_loan_monthly.monthly_repayment,
vw_loan_monthly.repayment_period, vw_loan_monthly.loan_balance,
vw_loan_monthly.start_date, vw_loan_monthly.interest_amount, vw_loan_monthly.interest_paid, vw_loan_monthly.repayment,
vw_contributions.payment_type_id,vw_contributions.deposit_date,vw_contributions.deposit_amount,
vw_contributions.entry_date,vw_contributions.transaction_ref,vw_contributions.contribution_amount,
vw_contributions.contribution_type_id,vw_contributions.contribution_type_name,vw_contributions.payment_type_name,
vw_contributions.payment_narrative, vw_investments.investment_id,vw_investments.investment_type_id,vw_investments.maturity_date,vw_investments.invest_amount,
    vw_investments.yearly_dividend,vw_investments.withdrawal_date,vw_investments.withdrwal_amount,vw_investments.period_years,
vw_investments.default_interest,vw_investments.return_on_investment,vw_investments.application_date,vw_investments.investment_type_name
FROM vw_loan_monthly
full outer join vw_contributions on vw_contributions.entity_id = vw_loan_monthly.entity_id
full outer join vw_investments on vw_investments.entity_id = vw_loan_monthly.entity_id
where vw_investments.approve_status = 'Approved' --(org_id = '$P!{filter_id}')
$P!{organd}
ORDER BY vw_loan_monthly.loan_date asc]]>
